[38;5;246;03m// Single-line comments start with //[39;00m

[38;5;246;03m/*[39;00m
[38;5;246;03mMulti-line comments look like this.[39;00m
[38;5;246;03m*/[39;00m

[38;5;246;03m/**[39;00m
[38;5;246;03m * JavaDoc comments look like this. Used to describe the Class or various[39;00m
[38;5;246;03m * attributes of a Class.[39;00m
[38;5;246;03m * Main attributes:[39;00m
[38;5;246;03m *[39;00m
[38;5;246;03m * @author         Name (and contact information such as email) of author(s).[39;00m
[38;5;246;03m * @version     Current version of the program.[39;00m
[38;5;246;03m * @since        When this part of the program was first added.[39;00m
[38;5;246;03m * @param         For describing the different parameters for a method.[39;00m
[38;5;246;03m * @return        For describing what the method returns.[39;00m
[38;5;246;03m * @deprecated  For showing the code is outdated or shouldn't be used.[39;00m
[38;5;246;03m * @see         Links to another part of documentation.[39;00m
[38;5;246;03m*/[39;00m

[38;5;246;03m// Import ArrayList class inside of the java.util package[39;00m
[38;5;70;01mimport[39;00m[38;5;252m [39m[38;5;68;04mjava.util.ArrayList[39;00m[38;5;252m;[39m
[38;5;246;03m// Import all classes inside of java.security package[39;00m
[38;5;70;01mimport[39;00m[38;5;252m [39m[38;5;68;04mjava.security.*[39;00m[38;5;252m;[39m

[38;5;70;01mpublic[39;00m[38;5;252m [39m[38;5;70;01mclass[39;00m[38;5;252m [39m[38;5;68;04mLearnJava[39;00m[38;5;252m [39m[38;5;252m{[39m

[38;5;252m    [39m[38;5;246;03m// In order to run a java program, it must have a main method as an entry[39;00m
[38;5;252m    [39m[38;5;246;03m// point.[39;00m
[38;5;252m    [39m[38;5;70;01mpublic[39;00m[38;5;252m [39m[38;5;70;01mstatic[39;00m[38;5;252m [39m[38;5;70;01mvoid[39;00m[38;5;252m [39m[38;5;68mmain[39m[38;5;252m([39m[38;5;252mString[39m[38;5;252m[[39m[38;5;252m][39m[38;5;252m [39m[38;5;252margs[39m[38;5;252m)[39m[38;5;252m [39m[38;5;252m{[39m

[38;5;252m    [39m[38;5;246;03m///////////////////////////////////////[39;00m
[38;5;252m    [39m[38;5;246;03m// Input/Output[39;00m
[38;5;252m    [39m[38;5;246;03m///////////////////////////////////////[39;00m

[38;5;252m        [39m[38;5;246;03m/*[39;00m
[38;5;246;03m        * Output[39;00m
[38;5;246;03m        */[39;00m

[38;5;252m        [39m[38;5;246;03m// Use System.out.println() to print lines.[39;00m
[38;5;252m        [39m[38;5;252mSystem[39m[38;5;252m.[39m[38;5;250mout[39m[38;5;252m.[39m[38;5;250mprintln[39m[38;5;252m([39m[38;5;214m"Hello World!"[39m[38;5;252m)[39m[38;5;252m;[39m
[38;5;252m        [39m[38;5;252mSystem[39m[38;5;252m.[39m[38;5;250mout[39m[38;5;252m.[39m[38;5;250mprintln[39m[38;5;252m([39m
[38;5;252m            [39m[38;5;214m"Integer: "[39m[38;5;252m [39m[38;5;252m+[39m[38;5;252m [39m[38;5;67m10[39m[38;5;252m [39m[38;5;252m+[39m
[38;5;252m            [39m[38;5;214m" Double: "[39m[38;5;252m [39m[38;5;252m+[39m[38;5;252m [39m[38;5;67m3.14[39m[38;5;252m [39m[38;5;252m+[39m
[38;5;252m            [39m[38;5;214m" Boolean: "[39m[38;5;252m [39m[38;5;252m+[39m[38;5;252m [39m[38;5;70;01mtrue[39;00m[38;5;252m)[39m[38;5;252m;[39m

[38;5;252m        [39m[38;5;246;03m// To print without a newline, use System.out.print().[39;00m
[38;5;252m        [39m[38;5;252mSystem[39m[38;5;252m.[39m[38;5;250mout[39m[38;5;252m.[39m[38;5;250mprint[39m[38;5;252m([39m[38;5;214m"Hello "[39m[38;5;252m)[39m[38;5;252m;[39m
[38;5;252m        [39m[38;5;252mSystem[39m[38;5;252m.[39m[38;5;250mout[39m[38;5;252m.[39m[38;5;250mprint[39m[38;5;252m([39m[38;5;214m"World"[39m[38;5;252m)[39m[38;5;252m;[39m

[38;5;252m        [39m[38;5;246;03m// Use System.out.printf() for easy formatted printing.[39;00m
[38;5;252m        [39m[38;5;252mSystem[39m[38;5;252m.[39m[38;5;250mout[39m[38;5;252m.[39m[38;5;250mprintf[39m[38;5;252m([39m[38;5;214m"pi = %.5f"[39m[38;5;252m,[39m[38;5;252m [39m[38;5;252mMath[39m[38;5;252m.[39m[38;5;250mPI[39m[38;5;252m)[39m[38;5;252m;[39m[38;5;252m [39m[38;5;246;03m// => pi = 3.14159[39;00m

[38;5;252m        [39m[38;5;246;03m/*[39;00m
[38;5;246;03m         * Input[39;00m
[38;5;246;03m         */[39;00m

[38;5;252m        [39m[38;5;246;03m// use Scanner to read input[39;00m
[38;5;252m        [39m[38;5;246;03m// must import java.util.Scanner;[39;00m
[38;5;252m        [39m[38;5;252mScanner[39m[38;5;252m [39m[38;5;252mscanner[39m[38;5;252m [39m[38;5;252m=[39m[38;5;252m [39m[38;5;70;01mnew[39;00m[38;5;252m [39m[38;5;252mScanner[39m[38;5;252m([39m[38;5;252mSystem[39m[38;5;252m.[39m[38;5;250min[39m[38;5;252m)[39m[38;5;252m;[39m

[38;5;252m        [39m[38;5;246;03m// read string input[39;00m
[38;5;252m        [39m[38;5;252mString[39m[38;5;252m [39m[38;5;252mname[39m[38;5;252m [39m[38;5;252m=[39m[38;5;252m [39m[38;5;252mscanner[39m[38;5;252m.[39m[38;5;250mnext[39m[38;5;252m([39m[38;5;252m)[39m[38;5;252m;[39m

[38;5;252m        [39m[38;5;246;03m// read byte input[39;00m
[38;5;252m        [39m[38;5;70;01mbyte[39;00m[38;5;252m [39m[38;5;252mnumByte[39m[38;5;252m [39m[38;5;252m=[39m[38;5;252m [39m[38;5;252mscanner[39m[38;5;252m.[39m[38;5;250mnextByte[39m[38;5;252m([39m[38;5;252m)[39m[38;5;252m;[39m

[38;5;252m        [39m[38;5;246;03m// read int input[39;00m
[38;5;252m        [39m[38;5;70;01mint[39;00m[38;5;252m [39m[38;5;252mnumInt[39m[38;5;252m [39m[38;5;252m=[39m[38;5;252m [39m[38;5;252mscanner[39m[38;5;252m.[39m[38;5;250mnextInt[39m[38;5;252m([39m[38;5;252m)[39m[38;5;252m;[39m

[38;5;252m        [39m[38;5;246;03m// read long input[39;00m
[38;5;252m        [39m[38;5;70;01mfloat[39;00m[38;5;252m [39m[38;5;252mnumFloat[39m[38;5;252m [39m[38;5;252m=[39m[38;5;252m [39m[38;5;252mscanner[39m[38;5;252m.[39m[38;5;250mnextFloat[39m[38;5;252m([39m[38;5;252m)[39m[38;5;252m;[39m

[38;5;252m        [39m[38;5;246;03m// read double input[39;00m
[38;5;252m        [39m[38;5;70;01mdouble[39;00m[38;5;252m [39m[38;5;252mnumDouble[39m[38;5;252m [39m[38;5;252m=[39m[38;5;252m [39m[38;5;252mscanner[39m[38;5;252m.[39m[38;5;250mnextDouble[39m[38;5;252m([39m[38;5;252m)[39m[38;5;252m;[39m

[38;5;252m        [39m[38;5;246;03m// read boolean input[39;00m
[38;5;252m        [39m[38;5;70;01mboolean[39;00m[38;5;252m [39m[38;5;252mbool[39m[38;5;252m [39m[38;5;252m=[39m[38;5;252m [39m[38;5;252mscanner[39m[38;5;252m.[39m[38;5;250mnextBoolean[39m[38;5;252m([39m[38;5;252m)[39m[38;5;252m;[39m

[38;5;252m        [39m[38;5;246;03m///////////////////////////////////////[39;00m
[38;5;252m        [39m[38;5;246;03m// Variables[39;00m
[38;5;252m        [39m[38;5;246;03m///////////////////////////////////////[39;00m

[38;5;252m        [39m[38;5;246;03m/*[39;00m
[38;5;246;03m        *  Variable Declaration[39;00m
[38;5;246;03m        */[39;00m
[38;5;252m        [39m[38;5;246;03m// Declare a variable using <type> <name>[39;00m
[38;5;252m        [39m[38;5;70;01mint[39;00m[38;5;252m [39m[38;5;252mfooInt[39m[38;5;252m;[39m
[38;5;252m        [39m[38;5;246;03m// Declare multiple variables of the same[39;00m
[38;5;252m        [39m[38;5;246;03m// type <type> <name1>, <name2>, <name3>[39;00m
[38;5;252m        [39m[38;5;70;01mint[39;00m[38;5;252m [39m[38;5;252mfooInt1[39m[38;5;252m,[39m[38;5;252m [39m[38;5;252mfooInt2[39m[38;5;252m,[39m[38;5;252m [39m[38;5;252mfooInt3[39m[38;5;252m;[39m

[38;5;252m        [39m[38;5;246;03m/*[39;00m
[38;5;246;03m        *  Variable Initialization[39;00m
[38;5;246;03m        */[39;00m

[38;5;252m        [39m[38;5;246;03m// Initialize a variable using <type> <name> = <val>[39;00m
[38;5;252m        [39m[38;5;70;01mint[39;00m[38;5;252m [39m[38;5;252mbarInt[39m[38;5;252m [39m[38;5;252m=[39m[38;5;252m [39m[38;5;67m1[39m[38;5;252m;[39m
[38;5;252m        [39m[38;5;246;03m// Initialize multiple variables of same type with same[39;00m
[38;5;252m        [39m[38;5;246;03m// value <type> <name1>, <name2>, <name3>[39;00m
[38;5;252m        [39m[38;5;246;03m// <name1> = <name2> = <name3> = <val>[39;00m
[38;5;252m        [39m[38;5;70;01mint[39;00m[38;5;252m [39m[38;5;252mbarInt1[39m[38;5;252m,[39m[38;5;252m [39m[38;5;252mbarInt2[39m[38;5;252m,[39m[38;5;252m [39m[38;5;252mbarInt3[39m[38;5;252m;[39m
[38;5;252m        [39m[38;5;252mbarInt1[39m[38;5;252m [39m[38;5;252m=[39m[38;5;252m [39m[38;5;252mbarInt2[39m[38;5;252m [39m[38;5;252m=[39m[38;5;252m [39m[38;5;252mbarInt3[39m[38;5;252m [39m[38;5;252m=[39m[38;5;252m [39m[38;5;67m1[39m[38;5;252m;[39m

[38;5;252m        [39m[38;5;246;03m/*[39;00m
[38;5;246;03m        *  Variable types[39;00m
[38;5;246;03m        */[39;00m
[38;5;252m        [39m[38;5;246;03m// Byte - 8-bit signed two's complement integer[39;00m
[38;5;252m        [39m[38;5;246;03m// (-128 <= byte <= 127)[39;00m
[38;5;252m        [39m[38;5;70;01mbyte[39;00m[38;5;252m [39m[38;5;252mfooByte[39m[38;5;252m [39m[38;5;252m=[39m[38;5;252m [39m[38;5;67m100[39m[38;5;252m;[39m

[38;5;252m        [39m[38;5;246;03m// If you would like to interpret a byte as an unsigned integer[39;00m
[38;5;252m        [39m[38;5;246;03m// then this simple operation can help[39;00m
[38;5;252m        [39m[38;5;70;01mint[39;00m[38;5;252m [39m[38;5;252munsignedIntLessThan256[39m[38;5;252m [39m[38;5;252m=[39m[38;5;252m [39m[38;5;67m0xff[39m[38;5;252m [39m[38;5;252m&[39m[38;5;252m [39m[38;5;252mfooByte[39m[38;5;252m;[39m
[38;5;252m        [39m[38;5;246;03m// this contrasts a cast which can be negative.[39;00m
[38;5;252m        [39m[38;5;70;01mint[39;00m[38;5;252m [39m[38;5;252msignedInt[39m[38;5;252m [39m[38;5;252m=[39m[38;5;252m [39m[38;5;252m([39m[38;5;70;01mint[39;00m[38;5;252m)[39m[38;5;252m [39m[38;5;252mfooByte[39m[38;5;252m;[39m

[38;5;252m        [39m[38;5;246;03m// Short - 16-bit signed two's complement integer[39;00m
[38;5;252m        [39m[38;5;246;03m// (-32,768 <= short <= 32,767)[39;00m
[38;5;252m        [39m[38;5;70;01mshort[39;00m[38;5;252m [39m[38;5;252mfooShort[39m[38;5;252m [39m[38;5;252m=[39m[38;5;252m [39m[38;5;67m10000[39m[38;5;252m;[39m

[38;5;252m        [39m[38;5;246;03m// Integer - 32-bit signed two's complement integer[39;00m
[38;5;252m        [39m[38;5;246;03m// (-2,147,483,648 <= int <= 2,147,483,647)[39;00m
[38;5;252m        [39m[38;5;70;01mint[39;00m[38;5;252m [39m[38;5;252mbazInt[39m[38;5;252m [39m[38;5;252m=[39m[38;5;252m [39m[38;5;67m1[39m[38;5;252m;[39m

[38;5;252m        [39m[38;5;246;03m// Long - 64-bit signed two's complement integer[39;00m
[38;5;252m        [39m[38;5;246;03m// (-9,223,372,036,854,775,808 <= long <= 9,223,372,036,854,775,807)[39;00m
[38;5;252m        [39m[38;5;70;01mlong[39;00m[38;5;252m [39m[38;5;252mfooLong[39m[38;5;252m [39m[38;5;252m=[39m[38;5;252m [39m[38;5;67m100000L[39m[38;5;252m;[39m
[38;5;252m        [39m[38;5;246;03m// L is used to denote that this variable value is of type Long;[39;00m
[38;5;252m        [39m[38;5;246;03m// anything without is treated as integer by default.[39;00m

[38;5;252m        [39m[38;5;246;03m// Note: byte, short, int and long are signed. They can have positive and negative values.[39;00m
[38;5;252m        [39m[38;5;246;03m// There are no unsigned variants.[39;00m
[38;5;252m        [39m[38;5;246;03m// char, however, is 16-bit unsigned.[39;00m

[38;5;252m        [39m[38;5;246;03m// Float - Single-precision 32-bit IEEE 754 Floating Point[39;00m
[38;5;252m        [39m[38;5;246;03m// 2^-149 <= float <= (2-2^-23) * 2^127[39;00m
[38;5;252m        [39m[38;5;70;01mfloat[39;00m[38;5;252m [39m[38;5;252mfooFloat[39m[38;5;252m [39m[38;5;252m=[39m[38;5;252m [39m[38;5;67m234.5f[39m[38;5;252m;[39m
[38;5;252m        [39m[38;5;246;03m// f or F is used to denote that this variable value is of type float;[39;00m
[38;5;252m        [39m[38;5;246;03m// otherwise it is treated as double.[39;00m

[38;5;252m        [39m[38;5;246;03m// Double - Double-precision 64-bit IEEE 754 Floating Point[39;00m
[38;5;252m        [39m[38;5;246;03m// 2^-1074 <= x <= (2-2^-52) * 2^1023[39;00m
[38;5;252m        [39m[38;5;70;01mdouble[39;00m[38;5;252m [39m[38;5;252mfooDouble[39m[38;5;252m [39m[38;5;252m=[39m[38;5;252m [39m[38;5;67m123.4[39m[38;5;252m;[39m

[38;5;252m        [39m[38;5;246;03m// Boolean - true & false[39;00m
[38;5;252m        [39m[38;5;70;01mboolean[39;00m[38;5;252m [39m[38;5;252mfooBoolean[39m[38;5;252m [39m[38;5;252m=[39m[38;5;252m [39m[38;5;70;01mtrue[39;00m[38;5;252m;[39m
[38;5;252m        [39m[38;5;70;01mboolean[39;00m[38;5;252m [39m[38;5;252mbarBoolean[39m[38;5;252m [39m[38;5;252m=[39m[38;5;252m [39m[38;5;70;01mfalse[39;00m[38;5;252m;[39m

[38;5;252m        [39m[38;5;246;03m// Char - A single 16-bit Unicode character[39;00m
[38;5;252m        [39m[38;5;70;01mchar[39;00m[38;5;252m [39m[38;5;252mfooChar[39m[38;5;252m [39m[38;5;252m=[39m[38;5;252m [39m[38;5;214m'A'[39m[38;5;252m;[39m

[38;5;252m        [39m[38;5;246;03m// final variables can't be reassigned,[39;00m
[38;5;252m        [39m[38;5;70;01mfinal[39;00m[38;5;252m [39m[38;5;70;01mint[39;00m[38;5;252m [39m[38;5;252mHOURS_I_WORK_PER_WEEK[39m[38;5;252m [39m[38;5;252m=[39m[38;5;252m [39m[38;5;67m9001[39m[38;5;252m;[39m
[38;5;252m        [39m[38;5;246;03m// but they can be initialized later.[39;00m
[38;5;252m        [39m[38;5;70;01mfinal[39;00m[38;5;252m [39m[38;5;70;01mdouble[39;00m[38;5;252m [39m[38;5;252mE[39m[38;5;252m;[39m
[38;5;252m        [39m[38;5;252mE[39m[38;5;252m [39m[38;5;252m=[39m[38;5;252m [39m[38;5;67m2.71828[39m[38;5;252m;[39m

[38;5;252m        [39m[38;5;246;03m// BigInteger - Immutable arbitrary-precision integers[39;00m
[38;5;252m        [39m[38;5;246;03m//[39;00m
[38;5;252m        [39m[38;5;246;03m// BigInteger is a data type that allows programmers to manipulate[39;00m
[38;5;252m        [39m[38;5;246;03m// integers longer than 64-bits. Integers are stored as an array of[39;00m
[38;5;252m        [39m[38;5;246;03m// of bytes and are manipulated using functions built into BigInteger[39;00m
[38;5;252m        [39m[38;5;246;03m//[39;00m
[38;5;252m        [39m[38;5;246;03m// BigInteger can be initialized using an array of bytes or a string.[39;00m
[38;5;252m        [39m[38;5;252mBigInteger[39m[38;5;252m [39m[38;5;252mfooBigInteger[39m[38;5;252m [39m[38;5;252m=[39m[38;5;252m [39m[38;5;70;01mnew[39;00m[38;5;252m [39m[38;5;252mBigInteger[39m[38;5;252m([39m[38;5;252mfooByteArray[39m[38;5;252m)[39m[38;5;252m;[39m

[38;5;252m        [39m[38;5;246;03m// BigDecimal - Immutable, arbitrary-precision signed decimal number[39;00m
[38;5;252m        [39m[38;5;246;03m//[39;00m
[38;5;252m        [39m[38;5;246;03m// A BigDecimal takes two parts: an arbitrary precision integer[39;00m
[38;5;252m        [39m[38;5;246;03m// unscaled value and a 32-bit integer scale[39;00m
[38;5;252m        [39m[38;5;246;03m//[39;00m
[38;5;252m        [39m[38;5;246;03m// BigDecimal allows the programmer complete control over decimal[39;00m
[38;5;252m        [39m[38;5;246;03m// rounding. It is recommended to use BigDecimal with currency values[39;00m
[38;5;252m        [39m[38;5;246;03m// and where exact decimal precision is required.[39;00m
[38;5;252m        [39m[38;5;246;03m//[39;00m
[38;5;252m        [39m[38;5;246;03m// BigDecimal can be initialized with an int, long, double or String[39;00m
[38;5;252m        [39m[38;5;246;03m// or by initializing the unscaled value (BigInteger) and scale (int).[39;00m
[38;5;252m        [39m[38;5;252mBigDecimal[39m[38;5;252m [39m[38;5;252mfooBigDecimal[39m[38;5;252m [39m[38;5;252m=[39m[38;5;252m [39m[38;5;70;01mnew[39;00m[38;5;252m [39m[38;5;252mBigDecimal[39m[38;5;252m([39m[38;5;252mfooBigInteger[39m[38;5;252m,[39m[38;5;252m [39m[38;5;252mfooInt[39m[38;5;252m)[39m[38;5;252m;[39m

[38;5;252m        [39m[38;5;246;03m// Be wary of the constructor that takes a float or double as[39;00m
[38;5;252m        [39m[38;5;246;03m// the inaccuracy of the float/double will be copied in BigDecimal.[39;00m
[38;5;252m        [39m[38;5;246;03m// Prefer the String constructor when you need an exact value.[39;00m
[38;5;252m        [39m[38;5;252mBigDecimal[39m[38;5;252m [39m[38;5;252mtenCents[39m[38;5;252m [39m[38;5;252m=[39m[38;5;252m [39m[38;5;70;01mnew[39;00m[38;5;252m [39m[38;5;252mBigDecimal[39m[38;5;252m([39m[38;5;214m"0.1"[39m[38;5;252m)[39m[38;5;252m;[39m

[38;5;252m        [39m[38;5;246;03m// Strings[39;00m
[38;5;252m        [39m[38;5;252mString[39m[38;5;252m [39m[38;5;252mfooString[39m[38;5;252m [39m[38;5;252m=[39m[38;5;252m [39m[38;5;214m"My String Is Here!"[39m[38;5;252m;[39m

[38;5;252m        [39m[38;5;246;03m// \n is an escaped character that starts a new line[39;00m
[38;5;252m        [39m[38;5;252mString[39m[38;5;252m [39m[38;5;252mbarString[39m[38;5;252m [39m[38;5;252m=[39m[38;5;252m [39m[38;5;214m"Printing on a new line?\nNo Problem!"[39m[38;5;252m;[39m
[38;5;252m        [39m[38;5;246;03m// \t is an escaped character that adds a tab character[39;00m
[38;5;252m        [39m[38;5;252mString[39m[38;5;252m [39m[38;5;252mbazString[39m[38;5;252m [39m[38;5;252m=[39m[38;5;252m [39m[38;5;214m"Do you want to add a tab?\tNo Problem!"[39m[38;5;252m;[39m
[38;5;252m        [39m[38;5;252mSystem[39m[38;5;252m.[39m[38;5;250mout[39m[38;5;252m.[39m[38;5;250mprintln[39m[38;5;252m([39m[38;5;252mfooString[39m[38;5;252m)[39m[38;5;252m;[39m
[38;5;252m        [39m[38;5;252mSystem[39m[38;5;252m.[39m[38;5;250mout[39m[38;5;252m.[39m[38;5;250mprintln[39m[38;5;252m([39m[38;5;252mbarString[39m[38;5;252m)[39m[38;5;252m;[39m
[38;5;252m        [39m[38;5;252mSystem[39m[38;5;252m.[39m[38;5;250mout[39m[38;5;252m.[39m[38;5;250mprintln[39m[38;5;252m([39m[38;5;252mbazString[39m[38;5;252m)[39m[38;5;252m;[39m

[38;5;252m        [39m[38;5;246;03m// String Building[39;00m
[38;5;252m        [39m[38;5;246;03m// #1 - with plus operator[39;00m
[38;5;252m        [39m[38;5;246;03m// That's the basic way to do it (optimized under the hood)[39;00m
[38;5;252m        [39m[38;5;252mString[39m[38;5;252m [39m[38;5;252mplusConcatenated[39m[38;5;252m [39m[38;5;252m=[39m[38;5;252m [39m[38;5;214m"Strings can "[39m[38;5;252m [39m[38;5;252m+[39m[38;5;252m [39m[38;5;214m"be concatenated "[39m[38;5;252m [39m[38;5;252m+[39m[38;5;252m [39m[38;5;214m"via + operator."[39m[38;5;252m;[39m
[38;5;252m        [39m[38;5;252mSystem[39m[38;5;252m.[39m[38;5;250mout[39m[38;5;252m.[39m[38;5;250mprintln[39m[38;5;252m([39m[38;5;252mplusConcatenated[39m[38;5;252m)[39m[38;5;252m;[39m
[38;5;252m        [39m[38;5;246;03m// Output: Strings can be concatenated via + operator.[39;00m

[38;5;252m        [39m[38;5;246;03m// #2 - with StringBuilder[39;00m
[38;5;252m        [39m[38;5;246;03m// This way doesn't create any intermediate strings. It just stores the string pieces, and ties them together[39;00m
[38;5;252m        [39m[38;5;246;03m// when toString() is called.[39;00m
[38;5;252m        [39m[38;5;246;03m// Hint: This class is not thread safe. A thread-safe alternative (with some impact on performance) is StringBuffer.[39;00m
[38;5;252m        [39m[38;5;252mStringBuilder[39m[38;5;252m [39m[38;5;252mbuilderConcatenated[39m[38;5;252m [39m[38;5;252m=[39m[38;5;252m [39m[38;5;70;01mnew[39;00m[38;5;252m [39m[38;5;252mStringBuilder[39m[38;5;252m([39m[38;5;252m)[39m[38;5;252m;[39m
[38;5;252m        [39m[38;5;252mbuilderConcatenated[39m[38;5;252m.[39m[38;5;250mappend[39m[38;5;252m([39m[38;5;214m"You "[39m[38;5;252m)[39m[38;5;252m;[39m
[38;5;252m        [39m[38;5;252mbuilderConcatenated[39m[38;5;252m.[39m[38;5;250mappend[39m[38;5;252m([39m[38;5;214m"can use "[39m[38;5;252m)[39m[38;5;252m;[39m
[38;5;252m        [39m[38;5;252mbuilderConcatenated[39m[38;5;252m.[39m[38;5;250mappend[39m[38;5;252m([39m[38;5;214m"the StringBuilder class."[39m[38;5;252m)[39m[38;5;252m;[39m
[38;5;252m        [39m[38;5;252mSystem[39m[38;5;252m.[39m[38;5;250mout[39m[38;5;252m.[39m[38;5;250mprintln[39m[38;5;252m([39m[38;5;252mbuilderConcatenated[39m[38;5;252m.[39m[38;5;250mtoString[39m[38;5;252m([39m[38;5;252m)[39m[38;5;252m)[39m[38;5;252m;[39m[38;5;252m [39m[38;5;246;03m// only now is the string built[39;00m
[38;5;252m        [39m[38;5;246;03m// Output: You can use the StringBuilder class.[39;00m

[38;5;252m        [39m[38;5;246;03m// StringBuilder is efficient when the fully constructed String is not required until the end of some processing.[39;00m
[38;5;252m        [39m[38;5;252mStringBuilder[39m[38;5;252m [39m[38;5;252mstringBuilder[39m[38;5;252m [39m[38;5;252m=[39m[38;5;252m [39m[38;5;70;01mnew[39;00m[38;5;252m [39m[38;5;252mStringBuilder[39m[38;5;252m([39m[38;5;252m)[39m[38;5;252m;[39m
[38;5;252m        [39m[38;5;252mString[39m[38;5;252m [39m[38;5;252minefficientString[39m[38;5;252m [39m[38;5;252m=[39m[38;5;252m [39m[38;5;214m""[39m[38;5;252m;[39m
[38;5;252m        [39m[38;5;70;01mfor[39;00m[38;5;252m [39m[38;5;252m([39m[38;5;70;01mint[39;00m[38;5;252m [39m[38;5;252mi[39m[38;5;252m [39m[38;5;252m=[39m[38;5;252m [39m[38;5;67m0[39m[38;5;252m [39m[38;5;252m;[39m[38;5;252m [39m[38;5;252mi[39m[38;5;252m [39m[38;5;252m<[39m[38;5;252m [39m[38;5;67m10[39m[38;5;252m;[39m[38;5;252m [39m[38;5;252mi[39m[38;5;252m+[39m[38;5;252m+[39m[38;5;252m)[39m[38;5;252m [39m[38;5;252m{[39m
[38;5;252m            [39m[38;5;252mstringBuilder[39m[38;5;252m.[39m[38;5;250mappend[39m[38;5;252m([39m[38;5;252mi[39m[38;5;252m)[39m[38;5;252m.[39m[38;5;250mappend[39m[38;5;252m([39m[38;5;214m" "[39m[38;5;252m)[39m[38;5;252m;[39m
[38;5;252m            [39m[38;5;252minefficientString[39m[38;5;252m [39m[38;5;252m+[39m[38;5;252m=[39m[38;5;252m [39m[38;5;252mi[39m[38;5;252m [39m[38;5;252m+[39m[38;5;252m [39m[38;5;214m" "[39m[38;5;252m;[39m
[38;5;252m        [39m[38;5;252m}[39m
[38;5;252m        [39m[38;5;252mSystem[39m[38;5;252m.[39m[38;5;250mout[39m[38;5;252m.[39m[38;5;250mprintln[39m[38;5;252m([39m[38;5;252minefficientString[39m[38;5;252m)[39m[38;5;252m;[39m
[38;5;252m        [39m[38;5;252mSystem[39m[38;5;252m.[39m[38;5;250mout[39m[38;5;252m.[39m[38;5;250mprintln[39m[38;5;252m([39m[38;5;252mstringBuilder[39m[38;5;252m.[39m[38;5;250mtoString[39m[38;5;252m([39m[38;5;252m)[39m[38;5;252m)[39m[38;5;252m;[39m
[38;5;252m        [39m[38;5;246;03m// inefficientString requires a lot more work to produce, as it generates a String on every loop iteration.[39;00m
[38;5;252m        [39m[38;5;246;03m// Simple concatenation with + is compiled to a StringBuilder and toString()[39;00m
[38;5;252m        [39m[38;5;246;03m// Avoid string concatenation in loops.[39;00m

[38;5;252m        [39m[38;5;246;03m// #3 - with String formatter[39;00m
[38;5;252m        [39m[38;5;246;03m// Another alternative way to create strings. Fast and readable.[39;00m
[38;5;252m        [39m[38;5;252mString[39m[38;5;252m.[39m[38;5;250mformat[39m[38;5;252m([39m[38;5;214m"%s may prefer %s."[39m[38;5;252m,[39m[38;5;252m [39m[38;5;214m"Or you"[39m[38;5;252m,[39m[38;5;252m [39m[38;5;214m"String.format()"[39m[38;5;252m)[39m[38;5;252m;[39m
[38;5;252m        [39m[38;5;246;03m// Output: Or you may prefer String.format().[39;00m

[38;5;252m        [39m[38;5;246;03m// Arrays[39;00m
[38;5;252m        [39m[38;5;246;03m// The array size must be decided upon instantiation[39;00m
[38;5;252m        [39m[38;5;246;03m// The following formats work for declaring an array[39;00m
[38;5;252m        [39m[38;5;246;03m// <datatype>[] <var name> = new <datatype>[<array size>];[39;00m
[38;5;252m        [39m[38;5;246;03m// <datatype> <var name>[] = new <datatype>[<array size>];[39;00m
[38;5;252m        [39m[38;5;70;01mint[39;00m[38;5;252m[[39m[38;5;252m][39m[38;5;252m [39m[38;5;252mintArray[39m[38;5;252m [39m[38;5;252m=[39m[38;5;252m [39m[38;5;70;01mnew[39;00m[38;5;252m [39m[38;5;70;01mint[39;00m[38;5;252m[[39m[38;5;67m10[39m[38;5;252m][39m[38;5;252m;[39m
[38;5;252m        [39m[38;5;252mString[39m[38;5;252m[[39m[38;5;252m][39m[38;5;252m [39m[38;5;252mstringArray[39m[38;5;252m [39m[38;5;252m=[39m[38;5;252m [39m[38;5;70;01mnew[39;00m[38;5;252m [39m[38;5;252mString[39m[38;5;252m[[39m[38;5;67m1[39m[38;5;252m][39m[38;5;252m;[39m
[38;5;252m        [39m[38;5;70;01mboolean[39;00m[38;5;252m [39m[38;5;252mboolArray[39m[38;5;252m[[39m[38;5;252m][39m[38;5;252m [39m[38;5;252m=[39m[38;5;252m [39m[38;5;70;01mnew[39;00m[38;5;252m [39m[38;5;70;01mboolean[39;00m[38;5;252m[[39m[38;5;67m100[39m[38;5;252m][39m[38;5;252m;[39m

[38;5;252m        [39m[38;5;246;03m// Another way to declare & initialize an array[39;00m
[38;5;252m        [39m[38;5;70;01mint[39;00m[38;5;252m[[39m[38;5;252m][39m[38;5;252m [39m[38;5;252my[39m[38;5;252m [39m[38;5;252m=[39m[38;5;252m [39m[38;5;252m{[39m[38;5;67m9000[39m[38;5;252m,[39m[38;5;252m [39m[38;5;67m1000[39m[38;5;252m,[39m[38;5;252m [39m[38;5;67m1337[39m[38;5;252m}[39m[38;5;252m;[39m
[38;5;252m        [39m[38;5;252mString[39m[38;5;252m [39m[38;5;252mnames[39m[38;5;252m[[39m[38;5;252m][39m[38;5;252m [39m[38;5;252m=[39m[38;5;252m [39m[38;5;252m{[39m[38;5;214m"Bob"[39m[38;5;252m,[39m[38;5;252m [39m[38;5;214m"John"[39m[38;5;252m,[39m[38;5;252m [39m[38;5;214m"Fred"[39m[38;5;252m,[39m[38;5;252m [39m[38;5;214m"Juan Pedro"[39m[38;5;252m}[39m[38;5;252m;[39m
[38;5;252m        [39m[38;5;70;01mboolean[39;00m[38;5;252m [39m[38;5;252mbools[39m[38;5;252m[[39m[38;5;252m][39m[38;5;252m [39m[38;5;252m=[39m[38;5;252m [39m[38;5;252m{[39m[38;5;70;01mtrue[39;00m[38;5;252m,[39m[38;5;252m [39m[38;5;70;01mfalse[39;00m[38;5;252m,[39m[38;5;252m [39m[38;5;70;01mfalse[39;00m[38;5;252m}[39m[38;5;252m;[39m

[38;5;252m        [39m[38;5;246;03m// Indexing an array - Accessing an element[39;00m
[38;5;252m        [39m[38;5;252mSystem[39m[38;5;252m.[39m[38;5;250mout[39m[38;5;252m.[39m[38;5;250mprintln[39m[38;5;252m([39m[38;5;214m"intArray @ 0: "[39m[38;5;252m [39m[38;5;252m+[39m[38;5;252m [39m[38;5;252mintArray[39m[38;5;252m[[39m[38;5;67m0[39m[38;5;252m][39m[38;5;252m)[39m[38;5;252m;[39m

[38;5;252m        [39m[38;5;246;03m// Arrays are zero-indexed and mutable.[39;00m
[38;5;252m        [39m[38;5;252mintArray[39m[38;5;252m[[39m[38;5;67m1[39m[38;5;252m][39m[38;5;252m [39m[38;5;252m=[39m[38;5;252m [39m[38;5;67m1[39m[38;5;252m;[39m
[38;5;252m        [39m[38;5;252mSystem[39m[38;5;252m.[39m[38;5;250mout[39m[38;5;252m.[39m[38;5;250mprintln[39m[38;5;252m([39m[38;5;214m"intArray @ 1: "[39m[38;5;252m [39m[38;5;252m+[39m[38;5;252m [39m[38;5;252mintArray[39m[38;5;252m[[39m[38;5;67m1[39m[38;5;252m][39m[38;5;252m)[39m[38;5;252m;[39m[38;5;252m [39m[38;5;246;03m// => 1[39;00m

[38;5;252m        [39m[38;5;246;03m// Other data types worth checking out[39;00m
[38;5;252m        [39m[38;5;246;03m// ArrayLists - Like arrays except more functionality is offered, and[39;00m
[38;5;252m        [39m[38;5;246;03m//              the size is mutable.[39;00m
[38;5;252m        [39m[38;5;246;03m// LinkedLists - Implementation of doubly-linked list. All of the[39;00m
[38;5;252m        [39m[38;5;246;03m//               operations perform as could be expected for a[39;00m
[38;5;252m        [39m[38;5;246;03m//               doubly-linked list.[39;00m
[38;5;252m        [39m[38;5;246;03m// Maps - A mapping of key Objects to value Objects. Map is[39;00m
[38;5;252m        [39m[38;5;246;03m//        an interface and therefore cannot be instantiated.[39;00m
[38;5;252m        [39m[38;5;246;03m//        The type of keys and values contained in a Map must[39;00m
[38;5;252m        [39m[38;5;246;03m//        be specified upon instantiation of the implementing[39;00m
[38;5;252m        [39m[38;5;246;03m//        class. Each key may map to only one corresponding value,[39;00m
[38;5;252m        [39m[38;5;246;03m//        and each key may appear only once (no duplicates).[39;00m
[38;5;252m        [39m[38;5;246;03m// HashMaps - This class uses a hashtable to implement the Map[39;00m
[38;5;252m        [39m[38;5;246;03m//            interface. This allows the execution time of basic[39;00m
[38;5;252m        [39m[38;5;246;03m//            operations, such as get and insert element, to remain[39;00m
[38;5;252m        [39m[38;5;246;03m//            constant-amortized even for large sets.[39;00m
[38;5;252m        [39m[38;5;246;03m// TreeMap - A Map that is sorted by its keys. Each modification[39;00m
[38;5;252m        [39m[38;5;246;03m//           maintains the sorting defined by either a Comparator[39;00m
[38;5;252m        [39m[38;5;246;03m//           supplied at instantiation, or comparisons of each Object[39;00m
[38;5;252m        [39m[38;5;246;03m//           if they implement the Comparable interface.[39;00m
[38;5;252m        [39m[38;5;246;03m//           Failure of keys to implement Comparable combined with failure to[39;00m
[38;5;252m        [39m[38;5;246;03m//           supply a Comparator will throw ClassCastExceptions.[39;00m
[38;5;252m        [39m[38;5;246;03m//           Insertion and removal operations take O(log(n)) time[39;00m
[38;5;252m        [39m[38;5;246;03m//           so avoid using this data structure unless you are taking[39;00m
[38;5;252m        [39m[38;5;246;03m//           advantage of the sorting.[39;00m

[38;5;252m        [39m[38;5;246;03m///////////////////////////////////////[39;00m
[38;5;252m        [39m[38;5;246;03m// Operators[39;00m
[38;5;252m        [39m[38;5;246;03m///////////////////////////////////////[39;00m
[38;5;252m        [39m[38;5;252mSystem[39m[38;5;252m.[39m[38;5;250mout[39m[38;5;252m.[39m[38;5;250mprintln[39m[38;5;252m([39m[38;5;214m"\n->Operators"[39m[38;5;252m)[39m[38;5;252m;[39m

[38;5;252m        [39m[38;5;70;01mint[39;00m[38;5;252m [39m[38;5;252mi1[39m[38;5;252m [39m[38;5;252m=[39m[38;5;252m [39m[38;5;67m1[39m[38;5;252m,[39m[38;5;252m [39m[38;5;252mi2[39m[38;5;252m [39m[38;5;252m=[39m[38;5;252m [39m[38;5;67m2[39m[38;5;252m;[39m[38;5;252m [39m[38;5;246;03m// Shorthand for multiple declarations[39;00m

[38;5;252m        [39m[38;5;246;03m// Arithmetic is straightforward[39;00m
[38;5;252m        [39m[38;5;252mSystem[39m[38;5;252m.[39m[38;5;250mout[39m[38;5;252m.[39m[38;5;250mprintln[39m[38;5;252m([39m[38;5;214m"1+2 = "[39m[38;5;252m [39m[38;5;252m+[39m[38;5;252m [39m[38;5;252m([39m[38;5;252mi1[39m[38;5;252m [39m[38;5;252m+[39m[38;5;252m [39m[38;5;252mi2[39m[38;5;252m)[39m[38;5;252m)[39m[38;5;252m;[39m[38;5;252m [39m[38;5;246;03m// => 3[39;00m
[38;5;252m        [39m[38;5;252mSystem[39m[38;5;252m.[39m[38;5;250mout[39m[38;5;252m.[39m[38;5;250mprintln[39m[38;5;252m([39m[38;5;214m"2-1 = "[39m[38;5;252m [39m[38;5;252m+[39m[38;5;252m [39m[38;5;252m([39m[38;5;252mi2[39m[38;5;252m [39m[38;5;252m-[39m[38;5;252m [39m[38;5;252mi1[39m[38;5;252m)[39m[38;5;252m)[39m[38;5;252m;[39m[38;5;252m [39m[38;5;246;03m// => 1[39;00m
[38;5;252m        [39m[38;5;252mSystem[39m[38;5;252m.[39m[38;5;250mout[39m[38;5;252m.[39m[38;5;250mprintln[39m[38;5;252m([39m[38;5;214m"2*1 = "[39m[38;5;252m [39m[38;5;252m+[39m[38;5;252m [39m[38;5;252m([39m[38;5;252mi2[39m[38;5;252m [39m[38;5;252m*[39m[38;5;252m [39m[38;5;252mi1[39m[38;5;252m)[39m[38;5;252m)[39m[38;5;252m;[39m[38;5;252m [39m[38;5;246;03m// => 2[39;00m
[38;5;252m        [39m[38;5;252mSystem[39m[38;5;252m.[39m[38;5;250mout[39m[38;5;252m.[39m[38;5;250mprintln[39m[38;5;252m([39m[38;5;214m"1/2 = "[39m[38;5;252m [39m[38;5;252m+[39m[38;5;252m [39m[38;5;252m([39m[38;5;252mi1[39m[38;5;252m [39m[38;5;252m/[39m[38;5;252m [39m[38;5;252mi2[39m[38;5;252m)[39m[38;5;252m)[39m[38;5;252m;[39m[38;5;252m [39m[38;5;246;03m// => 0 (int/int returns int)[39;00m
[38;5;252m        [39m[38;5;252mSystem[39m[38;5;252m.[39m[38;5;250mout[39m[38;5;252m.[39m[38;5;250mprintln[39m[38;5;252m([39m[38;5;214m"1/2.0 = "[39m[38;5;252m [39m[38;5;252m+[39m[38;5;252m [39m[38;5;252m([39m[38;5;252mi1[39m[38;5;252m [39m[38;5;252m/[39m[38;5;252m [39m[38;5;252m([39m[38;5;70;01mdouble[39;00m[38;5;252m)[39m[38;5;252mi2[39m[38;5;252m)[39m[38;5;252m)[39m[38;5;252m;[39m[38;5;252m [39m[38;5;246;03m// => 0.5[39;00m

[38;5;252m        [39m[38;5;246;03m// Modulo[39;00m
[38;5;252m        [39m[38;5;252mSystem[39m[38;5;252m.[39m[38;5;250mout[39m[38;5;252m.[39m[38;5;250mprintln[39m[38;5;252m([39m[38;5;214m"11%3 = "[39m[38;5;252m+[39m[38;5;252m([39m[38;5;67m11[39m[38;5;252m [39m[38;5;252m%[39m[38;5;252m [39m[38;5;67m3[39m[38;5;252m)[39m[38;5;252m)[39m[38;5;252m;[39m[38;5;252m [39m[38;5;246;03m// => 2[39;00m

[38;5;252m        [39m[38;5;246;03m// Comparison operators[39;00m
[38;5;252m        [39m[38;5;252mSystem[39m[38;5;252m.[39m[38;5;250mout[39m[38;5;252m.[39m[38;5;250mprintln[39m[38;5;252m([39m[38;5;214m"3 == 2? "[39m[38;5;252m [39m[38;5;252m+[39m[38;5;252m [39m[38;5;252m([39m[38;5;67m3[39m[38;5;252m [39m[38;5;252m=[39m[38;5;252m=[39m[38;5;252m [39m[38;5;67m2[39m[38;5;252m)[39m[38;5;252m)[39m[38;5;252m;[39m[38;5;252m [39m[38;5;246;03m// => false[39;00m
[38;5;252m        [39m[38;5;252mSystem[39m[38;5;252m.[39m[38;5;250mout[39m[38;5;252m.[39m[38;5;250mprintln[39m[38;5;252m([39m[38;5;214m"3 != 2? "[39m[38;5;252m [39m[38;5;252m+[39m[38;5;252m [39m[38;5;252m([39m[38;5;67m3[39m[38;5;252m [39m[38;5;252m![39m[38;5;252m=[39m[38;5;252m [39m[38;5;67m2[39m[38;5;252m)[39m[38;5;252m)[39m[38;5;252m;[39m[38;5;252m [39m[38;5;246;03m// => true[39;00m
[38;5;252m        [39m[38;5;252mSystem[39m[38;5;252m.[39m[38;5;250mout[39m[38;5;252m.[39m[38;5;250mprintln[39m[38;5;252m([39m[38;5;214m"3 > 2? "[39m[38;5;252m [39m[38;5;252m+[39m[38;5;252m [39m[38;5;252m([39m[38;5;67m3[39m[38;5;252m [39m[38;5;252m>[39m[38;5;252m [39m[38;5;67m2[39m[38;5;252m)[39m[38;5;252m)[39m[38;5;252m;[39m[38;5;252m [39m[38;5;246;03m// => true[39;00m
[38;5;252m        [39m[38;5;252mSystem[39m[38;5;252m.[39m[38;5;250mout[39m[38;5;252m.[39m[38;5;250mprintln[39m[38;5;252m([39m[38;5;214m"3 < 2? "[39m[38;5;252m [39m[38;5;252m+[39m[38;5;252m [39m[38;5;252m([39m[38;5;67m3[39m[38;5;252m [39m[38;5;252m<[39m[38;5;252m [39m[38;5;67m2[39m[38;5;252m)[39m[38;5;252m)[39m[38;5;252m;[39m[38;5;252m [39m[38;5;246;03m// => false[39;00m
[38;5;252m        [39m[38;5;252mSystem[39m[38;5;252m.[39m[38;5;250mout[39m[38;5;252m.[39m[38;5;250mprintln[39m[38;5;252m([39m[38;5;214m"2 <= 2? "[39m[38;5;252m [39m[38;5;252m+[39m[38;5;252m [39m[38;5;252m([39m[38;5;67m2[39m[38;5;252m [39m[38;5;252m<[39m[38;5;252m=[39m[38;5;252m [39m[38;5;67m2[39m[38;5;252m)[39m[38;5;252m)[39m[38;5;252m;[39m[38;5;252m [39m[38;5;246;03m// => true[39;00m
[38;5;252m        [39m[38;5;252mSystem[39m[38;5;252m.[39m[38;5;250mout[39m[38;5;252m.[39m[38;5;250mprintln[39m[38;5;252m([39m[38;5;214m"2 >= 2? "[39m[38;5;252m [39m[38;5;252m+[39m[38;5;252m [39m[38;5;252m([39m[38;5;67m2[39m[38;5;252m [39m[38;5;252m>[39m[38;5;252m=[39m[38;5;252m [39m[38;5;67m2[39m[38;5;252m)[39m[38;5;252m)[39m[38;5;252m;[39m[38;5;252m [39m[38;5;246;03m// => true[39;00m

[38;5;252m        [39m[38;5;246;03m// Boolean operators[39;00m
[38;5;252m        [39m[38;5;252mSystem[39m[38;5;252m.[39m[38;5;250mout[39m[38;5;252m.[39m[38;5;250mprintln[39m[38;5;252m([39m[38;5;214m"3 > 2 && 2 > 3? "[39m[38;5;252m [39m[38;5;252m+[39m[38;5;252m [39m[38;5;252m([39m[38;5;252m([39m[38;5;67m3[39m[38;5;252m [39m[38;5;252m>[39m[38;5;252m [39m[38;5;67m2[39m[38;5;252m)[39m[38;5;252m [39m[38;5;252m&[39m[38;5;252m&[39m[38;5;252m [39m[38;5;252m([39m[38;5;67m2[39m[38;5;252m [39m[38;5;252m>[39m[38;5;252m [39m[38;5;67m3[39m[38;5;252m)[39m[38;5;252m)[39m[38;5;252m)[39m[38;5;252m;[39m[38;5;252m [39m[38;5;246;03m// => false[39;00m
[38;5;252m        [39m[38;5;252mSystem[39m[38;5;252m.[39m[38;5;250mout[39m[38;5;252m.[39m[38;5;250mprintln[39m[38;5;252m([39m[38;5;214m"3 > 2 || 2 > 3? "[39m[38;5;252m [39m[38;5;252m+[39m[38;5;252m [39m[38;5;252m([39m[38;5;252m([39m[38;5;67m3[39m[38;5;252m [39m[38;5;252m>[39m[38;5;252m [39m[38;5;67m2[39m[38;5;252m)[39m[38;5;252m [39m[38;5;252m|[39m[38;5;252m|[39m[38;5;252m [39m[38;5;252m([39m[38;5;67m2[39m[38;5;252m [39m[38;5;252m>[39m[38;5;252m [39m[38;5;67m3[39m[38;5;252m)[39m[38;5;252m)[39m[38;5;252m)[39m[38;5;252m;[39m[38;5;252m [39m[38;5;246;03m// => true[39;00m
[38;5;252m        [39m[38;5;252mSystem[39m[38;5;252m.[39m[38;5;250mout[39m[38;5;252m.[39m[38;5;250mprintln[39m[38;5;252m([39m[38;5;214m"!(3 == 2)? "[39m[38;5;252m [39m[38;5;252m+[39m[38;5;252m [39m[38;5;252m([39m[38;5;252m![39m[38;5;252m([39m[38;5;67m3[39m[38;5;252m [39m[38;5;252m=[39m[38;5;252m=[39m[38;5;252m [39m[38;5;67m2[39m[38;5;252m)[39m[38;5;252m)[39m[38;5;252m)[39m[38;5;252m;[39m[38;5;252m [39m[38;5;246;03m// => true[39;00m

[38;5;252m        [39m[38;5;246;03m// Bitwise operators![39;00m
[38;5;252m        [39m[38;5;246;03m/*[39;00m
[38;5;246;03m        ~      Unary bitwise complement[39;00m
[38;5;246;03m        <<     Signed left shift[39;00m
[38;5;246;03m        >>     Signed/Arithmetic right shift[39;00m
[38;5;246;03m        >>>    Unsigned/Logical right shift[39;00m
[38;5;246;03m        &      Bitwise AND[39;00m
[38;5;246;03m        ^      Bitwise exclusive OR[39;00m
[38;5;246;03m        |      Bitwise inclusive OR[39;00m
[38;5;246;03m        */[39;00m

[38;5;252m        [39m[38;5;246;03m// Increment operators[39;00m
[38;5;252m        [39m[38;5;70;01mint[39;00m[38;5;252m [39m[38;5;252mi[39m[38;5;252m [39m[38;5;252m=[39m[38;5;252m [39m[38;5;67m0[39m[38;5;252m;[39m
[38;5;252m        [39m[38;5;252mSystem[39m[38;5;252m.[39m[38;5;250mout[39m[38;5;252m.[39m[38;5;250mprintln[39m[38;5;252m([39m[38;5;214m"\n->Inc/Dec-rementation"[39m[38;5;252m)[39m[38;5;252m;[39m
[38;5;252m        [39m[38;5;246;03m// The ++ and -- operators increment and decrement by 1 respectively.[39;00m
[38;5;252m        [39m[38;5;246;03m// If they are placed before the variable, they increment then return;[39;00m
[38;5;252m        [39m[38;5;246;03m// after the variable they return then increment.[39;00m
[38;5;252m        [39m[38;5;252mSystem[39m[38;5;252m.[39m[38;5;250mout[39m[38;5;252m.[39m[38;5;250mprintln[39m[38;5;252m([39m[38;5;252mi[39m[38;5;252m+[39m[38;5;252m+[39m[38;5;252m)[39m[38;5;252m;[39m[38;5;252m [39m[38;5;246;03m// i = 1, prints 0 (post-increment)[39;00m
[38;5;252m        [39m[38;5;252mSystem[39m[38;5;252m.[39m[38;5;250mout[39m[38;5;252m.[39m[38;5;250mprintln[39m[38;5;252m([39m[38;5;252m+[39m[38;5;252m+[39m[38;5;252mi[39m[38;5;252m)[39m[38;5;252m;[39m[38;5;252m [39m[38;5;246;03m// i = 2, prints 2 (pre-increment)[39;00m
[38;5;252m        [39m[38;5;252mSystem[39m[38;5;252m.[39m[38;5;250mout[39m[38;5;252m.[39m[38;5;250mprintln[39m[38;5;252m([39m[38;5;252mi[39m[38;5;252m-[39m[38;5;252m-[39m[38;5;252m)[39m[38;5;252m;[39m[38;5;252m [39m[38;5;246;03m// i = 1, prints 2 (post-decrement)[39;00m
[38;5;252m        [39m[38;5;252mSystem[39m[38;5;252m.[39m[38;5;250mout[39m[38;5;252m.[39m[38;5;250mprintln[39m[38;5;252m([39m[38;5;252m-[39m[38;5;252m-[39m[38;5;252mi[39m[38;5;252m)[39m[38;5;252m;[39m[38;5;252m [39m[38;5;246;03m// i = 0, prints 0 (pre-decrement)[39;00m

[38;5;252m        [39m[38;5;246;03m///////////////////////////////////////[39;00m
[38;5;252m        [39m[38;5;246;03m// Control Structures[39;00m
[38;5;252m        [39m[38;5;246;03m///////////////////////////////////////[39;00m
[38;5;252m        [39m[38;5;252mSystem[39m[38;5;252m.[39m[38;5;250mout[39m[38;5;252m.[39m[38;5;250mprintln[39m[38;5;252m([39m[38;5;214m"\n->Control Structures"[39m[38;5;252m)[39m[38;5;252m;[39m

[38;5;252m        [39m[38;5;246;03m// If statements are c-like[39;00m
[38;5;252m        [39m[38;5;70;01mint[39;00m[38;5;252m [39m[38;5;252mj[39m[38;5;252m [39m[38;5;252m=[39m[38;5;252m [39m[38;5;67m10[39m[38;5;252m;[39m
[38;5;252m        [39m[38;5;70;01mif[39;00m[38;5;252m [39m[38;5;252m([39m[38;5;252mj[39m[38;5;252m [39m[38;5;252m=[39m[38;5;252m=[39m[38;5;252m [39m[38;5;67m10[39m[38;5;252m)[39m[38;5;252m [39m[38;5;252m{[39m
[38;5;252m            [39m[38;5;252mSystem[39m[38;5;252m.[39m[38;5;250mout[39m[38;5;252m.[39m[38;5;250mprintln[39m[38;5;252m([39m[38;5;214m"I get printed"[39m[38;5;252m)[39m[38;5;252m;[39m
[38;5;252m        [39m[38;5;252m}[39m[38;5;252m [39m[38;5;70;01melse[39;00m[38;5;252m [39m[38;5;70;01mif[39;00m[38;5;252m [39m[38;5;252m([39m[38;5;252mj[39m[38;5;252m [39m[38;5;252m>[39m[38;5;252m [39m[38;5;67m10[39m[38;5;252m)[39m[38;5;252m [39m[38;5;252m{[39m
[38;5;252m            [39m[38;5;252mSystem[39m[38;5;252m.[39m[38;5;250mout[39m[38;5;252m.[39m[38;5;250mprintln[39m[38;5;252m([39m[38;5;214m"I don't"[39m[38;5;252m)[39m[38;5;252m;[39m
[38;5;252m        [39m[38;5;252m}[39m[38;5;252m [39m[38;5;70;01melse[39;00m[38;5;252m [39m[38;5;252m{[39m
[38;5;252m            [39m[38;5;252mSystem[39m[38;5;252m.[39m[38;5;250mout[39m[38;5;252m.[39m[38;5;250mprintln[39m[38;5;252m([39m[38;5;214m"I also don't"[39m[38;5;252m)[39m[38;5;252m;[39m
[38;5;252m        [39m[38;5;252m}[39m

[38;5;252m        [39m[38;5;246;03m// While loop[39;00m
[38;5;252m        [39m[38;5;70;01mint[39;00m[38;5;252m [39m[38;5;252mfooWhile[39m[38;5;252m [39m[38;5;252m=[39m[38;5;252m [39m[38;5;67m0[39m[38;5;252m;[39m
[38;5;252m        [39m[38;5;70;01mwhile[39;00m[38;5;252m([39m[38;5;252mfooWhile[39m[38;5;252m [39m[38;5;252m<[39m[38;5;252m [39m[38;5;67m100[39m[38;5;252m)[39m[38;5;252m [39m[38;5;252m{[39m
[38;5;252m            [39m[38;5;252mSystem[39m[38;5;252m.[39m[38;5;250mout[39m[38;5;252m.[39m[38;5;250mprintln[39m[38;5;252m([39m[38;5;252mfooWhile[39m[38;5;252m)[39m[38;5;252m;[39m
[38;5;252m            [39m[38;5;246;03m// Increment the counter[39;00m
[38;5;252m            [39m[38;5;246;03m// Iterated 100 times, fooWhile 0,1,2...99[39;00m
[38;5;252m            [39m[38;5;252mfooWhile[39m[38;5;252m+[39m[38;5;252m+[39m[38;5;252m;[39m
[38;5;252m        [39m[38;5;252m}[39m
[38;5;252m        [39m[38;5;252mSystem[39m[38;5;252m.[39m[38;5;250mout[39m[38;5;252m.[39m[38;5;250mprintln[39m[38;5;252m([39m[38;5;214m"fooWhile Value: "[39m[38;5;252m [39m[38;5;252m+[39m[38;5;252m [39m[38;5;252mfooWhile[39m[38;5;252m)[39m[38;5;252m;[39m

[38;5;252m        [39m[38;5;246;03m// Do While Loop[39;00m
[38;5;252m        [39m[38;5;70;01mint[39;00m[38;5;252m [39m[38;5;252mfooDoWhile[39m[38;5;252m [39m[38;5;252m=[39m[38;5;252m [39m[38;5;67m0[39m[38;5;252m;[39m
[38;5;252m        [39m[38;5;70;01mdo[39;00m[38;5;252m [39m[38;5;252m{[39m
[38;5;252m            [39m[38;5;252mSystem[39m[38;5;252m.[39m[38;5;250mout[39m[38;5;252m.[39m[38;5;250mprintln[39m[38;5;252m([39m[38;5;252mfooDoWhile[39m[38;5;252m)[39m[38;5;252m;[39m
[38;5;252m            [39m[38;5;246;03m// Increment the counter[39;00m
[38;5;252m            [39m[38;5;246;03m// Iterated 100 times, fooDoWhile 0->99[39;00m
[38;5;252m            [39m[38;5;252mfooDoWhile[39m[38;5;252m+[39m[38;5;252m+[39m[38;5;252m;[39m
[38;5;252m        [39m[38;5;252m}[39m[38;5;252m [39m[38;5;70;01mwhile[39;00m[38;5;252m([39m[38;5;252mfooDoWhile[39m[38;5;252m [39m[38;5;252m<[39m[38;5;252m [39m[38;5;67m100[39m[38;5;252m)[39m[38;5;252m;[39m
[38;5;252m        [39m[38;5;252mSystem[39m[38;5;252m.[39m[38;5;250mout[39m[38;5;252m.[39m[38;5;250mprintln[39m[38;5;252m([39m[38;5;214m"fooDoWhile Value: "[39m[38;5;252m [39m[38;5;252m+[39m[38;5;252m [39m[38;5;252mfooDoWhile[39m[38;5;252m)[39m[38;5;252m;[39m

[38;5;252m        [39m[38;5;246;03m// For Loop[39;00m
[38;5;252m        [39m[38;5;246;03m// for loop structure => for(<start_statement>; <conditional>; <step>)[39;00m
[38;5;252m        [39m[38;5;70;01mfor[39;00m[38;5;252m [39m[38;5;252m([39m[38;5;70;01mint[39;00m[38;5;252m [39m[38;5;252mfooFor[39m[38;5;252m [39m[38;5;252m=[39m[38;5;252m [39m[38;5;67m0[39m[38;5;252m;[39m[38;5;252m [39m[38;5;252mfooFor[39m[38;5;252m [39m[38;5;252m<[39m[38;5;252m [39m[38;5;67m10[39m[38;5;252m;[39m[38;5;252m [39m[38;5;252mfooFor[39m[38;5;252m+[39m[38;5;252m+[39m[38;5;252m)[39m[38;5;252m [39m[38;5;252m{[39m
[38;5;252m            [39m[38;5;252mSystem[39m[38;5;252m.[39m[38;5;250mout[39m[38;5;252m.[39m[38;5;250mprintln[39m[38;5;252m([39m[38;5;252mfooFor[39m[38;5;252m)[39m[38;5;252m;[39m
[38;5;252m            [39m[38;5;246;03m// Iterated 10 times, fooFor 0->9[39;00m
[38;5;252m        [39m[38;5;252m}[39m
[38;5;252m        [39m[38;5;252mSystem[39m[38;5;252m.[39m[38;5;250mout[39m[38;5;252m.[39m[38;5;250mprintln[39m[38;5;252m([39m[38;5;214m"fooFor Value: "[39m[38;5;252m [39m[38;5;252m+[39m[38;5;252m [39m[38;5;252mfooFor[39m[38;5;252m)[39m[38;5;252m;[39m

[38;5;252m        [39m[38;5;246;03m// Nested For Loop Exit with Label[39;00m
[38;5;252m        [39m[38;5;252mouter[39m[38;5;252m:[39m
[38;5;252m        [39m[38;5;70;01mfor[39;00m[38;5;252m [39m[38;5;252m([39m[38;5;70;01mint[39;00m[38;5;252m [39m[38;5;252mi[39m[38;5;252m [39m[38;5;252m=[39m[38;5;252m [39m[38;5;67m0[39m[38;5;252m;[39m[38;5;252m [39m[38;5;252mi[39m[38;5;252m [39m[38;5;252m<[39m[38;5;252m [39m[38;5;67m10[39m[38;5;252m;[39m[38;5;252m [39m[38;5;252mi[39m[38;5;252m+[39m[38;5;252m+[39m[38;5;252m)[39m[38;5;252m [39m[38;5;252m{[39m
[38;5;252m          [39m[38;5;70;01mfor[39;00m[38;5;252m [39m[38;5;252m([39m[38;5;70;01mint[39;00m[38;5;252m [39m[38;5;252mj[39m[38;5;252m [39m[38;5;252m=[39m[38;5;252m [39m[38;5;67m0[39m[38;5;252m;[39m[38;5;252m [39m[38;5;252mj[39m[38;5;252m [39m[38;5;252m<[39m[38;5;252m [39m[38;5;67m10[39m[38;5;252m;[39m[38;5;252m [39m[38;5;252mj[39m[38;5;252m+[39m[38;5;252m+[39m[38;5;252m)[39m[38;5;252m [39m[38;5;252m{[39m
[38;5;252m            [39m[38;5;70;01mif[39;00m[38;5;252m [39m[38;5;252m([39m[38;5;252mi[39m[38;5;252m [39m[38;5;252m=[39m[38;5;252m=[39m[38;5;252m [39m[38;5;67m5[39m[38;5;252m [39m[38;5;252m&[39m[38;5;252m&[39m[38;5;252m [39m[38;5;252mj[39m[38;5;252m [39m[38;5;252m=[39m[38;5;252m=[39m[38;5;67m5[39m[38;5;252m)[39m[38;5;252m [39m[38;5;252m{[39m
[38;5;252m              [39m[38;5;70;01mbreak[39;00m[38;5;252m [39m[38;5;252mouter[39m[38;5;252m;[39m
[38;5;252m              [39m[38;5;246;03m// breaks out of outer loop instead of only the inner one[39;00m
[38;5;252m            [39m[38;5;252m}[39m
[38;5;252m          [39m[38;5;252m}[39m
[38;5;252m        [39m[38;5;252m}[39m

[38;5;252m        [39m[38;5;246;03m// For Each Loop[39;00m
[38;5;252m        [39m[38;5;246;03m// The for loop is also able to iterate over arrays as well as objects[39;00m
[38;5;252m        [39m[38;5;246;03m// that implement the Iterable interface.[39;00m
[38;5;252m        [39m[38;5;70;01mint[39;00m[38;5;252m[[39m[38;5;252m][39m[38;5;252m [39m[38;5;252mfooList[39m[38;5;252m [39m[38;5;252m=[39m[38;5;252m [39m[38;5;252m{[39m[38;5;67m1[39m[38;5;252m,[39m[38;5;252m [39m[38;5;67m2[39m[38;5;252m,[39m[38;5;252m [39m[38;5;67m3[39m[38;5;252m,[39m[38;5;252m [39m[38;5;67m4[39m[38;5;252m,[39m[38;5;252m [39m[38;5;67m5[39m[38;5;252m,[39m[38;5;252m [39m[38;5;67m6[39m[38;5;252m,[39m[38;5;252m [39m[38;5;67m7[39m[38;5;252m,[39m[38;5;252m [39m[38;5;67m8[39m[38;5;252m,[39m[38;5;252m [39m[38;5;67m9[39m[38;5;252m}[39m[38;5;252m;[39m
[38;5;252m        [39m[38;5;246;03m// for each loop structure => for (<object> : <iterable>)[39;00m
[38;5;252m        [39m[38;5;246;03m// reads as: for each element in the iterable[39;00m
[38;5;252m        [39m[38;5;246;03m// note: the object type must match the element type of the iterable.[39;00m
[38;5;252m        [39m[38;5;70;01mfor[39;00m[38;5;252m [39m[38;5;252m([39m[38;5;70;01mint[39;00m[38;5;252m [39m[38;5;252mbar[39m[38;5;252m [39m[38;5;252m:[39m[38;5;252m [39m[38;5;252mfooList[39m[38;5;252m)[39m[38;5;252m [39m[38;5;252m{[39m
[38;5;252m            [39m[38;5;252mSystem[39m[38;5;252m.[39m[38;5;250mout[39m[38;5;252m.[39m[38;5;250mprintln[39m[38;5;252m([39m[38;5;252mbar[39m[38;5;252m)[39m[38;5;252m;[39m
[38;5;252m            [39m[38;5;246;03m//Iterates 9 times and prints 1-9 on new lines[39;00m
[38;5;252m        [39m[38;5;252m}[39m

[38;5;252m        [39m[38;5;246;03m// Switch Case[39;00m
[38;5;252m        [39m[38;5;246;03m// A switch works with the byte, short, char, and int data types.[39;00m
[38;5;252m        [39m[38;5;246;03m// It also works with enumerated types (discussed in Enum Types), the[39;00m
[38;5;252m        [39m[38;5;246;03m// String class, and a few special classes that wrap primitive types:[39;00m
[38;5;252m        [39m[38;5;246;03m// Character, Byte, Short, and Integer.[39;00m
[38;5;252m        [39m[38;5;246;03m// Starting in Java 7 and above, we can also use the String type.[39;00m
[38;5;252m        [39m[38;5;246;03m// Note: Do remember that, not adding "break" at end any particular case ends up in[39;00m
[38;5;252m        [39m[38;5;246;03m// executing the very next case(given it satisfies the condition provided) as well.[39;00m
[38;5;252m        [39m[38;5;70;01mint[39;00m[38;5;252m [39m[38;5;252mmonth[39m[38;5;252m [39m[38;5;252m=[39m[38;5;252m [39m[38;5;67m3[39m[38;5;252m;[39m
[38;5;252m        [39m[38;5;252mString[39m[38;5;252m [39m[38;5;252mmonthString[39m[38;5;252m;[39m
[38;5;252m        [39m[38;5;70;01mswitch[39;00m[38;5;252m [39m[38;5;252m([39m[38;5;252mmonth[39m[38;5;252m)[39m[38;5;252m [39m[38;5;252m{[39m
[38;5;252m            [39m[38;5;70;01mcase[39;00m[38;5;252m [39m[38;5;67m1[39m[38;5;252m:[39m[38;5;252m [39m[38;5;252mmonthString[39m[38;5;252m [39m[38;5;252m=[39m[38;5;252m [39m[38;5;214m"January"[39m[38;5;252m;[39m
[38;5;252m                    [39m[38;5;70;01mbreak[39;00m[38;5;252m;[39m
[38;5;252m            [39m[38;5;70;01mcase[39;00m[38;5;252m [39m[38;5;67m2[39m[38;5;252m:[39m[38;5;252m [39m[38;5;252mmonthString[39m[38;5;252m [39m[38;5;252m=[39m[38;5;252m [39m[38;5;214m"February"[39m[38;5;252m;[39m
[38;5;252m                    [39m[38;5;70;01mbreak[39;00m[38;5;252m;[39m
[38;5;252m            [39m[38;5;70;01mcase[39;00m[38;5;252m [39m[38;5;67m3[39m[38;5;252m:[39m[38;5;252m [39m[38;5;252mmonthString[39m[38;5;252m [39m[38;5;252m=[39m[38;5;252m [39m[38;5;214m"March"[39m[38;5;252m;[39m
[38;5;252m                    [39m[38;5;70;01mbreak[39;00m[38;5;252m;[39m
[38;5;252m            [39m[38;5;70;01mdefault[39;00m[38;5;252m:[39m[38;5;252m [39m[38;5;252mmonthString[39m[38;5;252m [39m[38;5;252m=[39m[38;5;252m [39m[38;5;214m"Some other month"[39m[38;5;252m;[39m
[38;5;252m                     [39m[38;5;70;01mbreak[39;00m[38;5;252m;[39m
[38;5;252m        [39m[38;5;252m}[39m
[38;5;252m        [39m[38;5;252mSystem[39m[38;5;252m.[39m[38;5;250mout[39m[38;5;252m.[39m[38;5;250mprintln[39m[38;5;252m([39m[38;5;214m"Switch Case Result: "[39m[38;5;252m [39m[38;5;252m+[39m[38;5;252m [39m[38;5;252mmonthString[39m[38;5;252m)[39m[38;5;252m;[39m


[38;5;252m        [39m[38;5;246;03m// Try-with-resources (Java 7+)[39;00m
[38;5;252m        [39m[38;5;246;03m// Try-catch-finally statements work as expected in Java but in Java 7+[39;00m
[38;5;252m        [39m[38;5;246;03m// the try-with-resources statement is also available. Try-with-resources[39;00m
[38;5;252m        [39m[38;5;246;03m// simplifies try-catch-finally statements by closing resources[39;00m
[38;5;252m        [39m[38;5;246;03m// automatically.[39;00m

[38;5;252m        [39m[38;5;246;03m// In order to use a try-with-resources, include an instance of a class[39;00m
[38;5;252m        [39m[38;5;246;03m// in the try statement. The class must implement java.lang.AutoCloseable.[39;00m
[38;5;252m        [39m[38;5;70;01mtry[39;00m[38;5;252m [39m[38;5;252m([39m[38;5;252mBufferedReader[39m[38;5;252m [39m[38;5;252mbr[39m[38;5;252m [39m[38;5;252m=[39m[38;5;252m [39m[38;5;70;01mnew[39;00m[38;5;252m [39m[38;5;252mBufferedReader[39m[38;5;252m([39m[38;5;70;01mnew[39;00m[38;5;252m [39m[38;5;252mFileReader[39m[38;5;252m([39m[38;5;214m"foo.txt"[39m[38;5;252m)[39m[38;5;252m)[39m[38;5;252m)[39m[38;5;252m [39m[38;5;252m{[39m
[38;5;252m            [39m[38;5;246;03m// You can attempt to do something that could throw an exception.[39;00m
[38;5;252m            [39m[38;5;252mSystem[39m[38;5;252m.[39m[38;5;250mout[39m[38;5;252m.[39m[38;5;250mprintln[39m[38;5;252m([39m[38;5;252mbr[39m[38;5;252m.[39m[38;5;250mreadLine[39m[38;5;252m([39m[38;5;252m)[39m[38;5;252m)[39m[38;5;252m;[39m
[38;5;252m            [39m[38;5;246;03m// In Java 7, the resource will always be closed, even if it throws[39;00m
[38;5;252m            [39m[38;5;246;03m// an Exception.[39;00m
[38;5;252m        [39m[38;5;252m}[39m[38;5;252m [39m[38;5;70;01mcatch[39;00m[38;5;252m [39m[38;5;252m([39m[38;5;252mException[39m[38;5;252m [39m[38;5;252mex[39m[38;5;252m)[39m[38;5;252m [39m[38;5;252m{[39m
[38;5;252m            [39m[38;5;246;03m//The resource will be closed before the catch statement executes.[39;00m
[38;5;252m            [39m[38;5;252mSystem[39m[38;5;252m.[39m[38;5;250mout[39m[38;5;252m.[39m[38;5;250mprintln[39m[38;5;252m([39m[38;5;214m"readLine() failed."[39m[38;5;252m)[39m[38;5;252m;[39m
[38;5;252m        [39m[38;5;252m}[39m
[38;5;252m        [39m[38;5;246;03m// No need for a finally statement in this case, the BufferedReader is[39;00m
[38;5;252m        [39m[38;5;246;03m// already closed. This can be used to avoid certain edge cases where[39;00m
[38;5;252m        [39m[38;5;246;03m// a finally statement might not be called.[39;00m
[38;5;252m        [39m[38;5;246;03m// To learn more:[39;00m
[38;5;252m        [39m[38;5;246;03m// https://docs.oracle.com/javase/tutorial/essential/exceptions/tryResourceClose.html[39;00m


[38;5;252m        [39m[38;5;246;03m// Conditional Shorthand[39;00m
[38;5;252m        [39m[38;5;246;03m// You can use the '?' operator for quick assignments or logic forks.[39;00m
[38;5;252m        [39m[38;5;246;03m// Reads as "If (statement) is true, use <first value>, otherwise, use[39;00m
[38;5;252m        [39m[38;5;246;03m// <second value>"[39;00m
[38;5;252m        [39m[38;5;70;01mint[39;00m[38;5;252m [39m[38;5;252mfoo[39m[38;5;252m [39m[38;5;252m=[39m[38;5;252m [39m[38;5;67m5[39m[38;5;252m;[39m
[38;5;252m        [39m[38;5;252mString[39m[38;5;252m [39m[38;5;252mbar[39m[38;5;252m [39m[38;5;252m=[39m[38;5;252m [39m[38;5;252m([39m[38;5;252mfoo[39m[38;5;252m [39m[38;5;252m<[39m[38;5;252m [39m[38;5;67m10[39m[38;5;252m)[39m[38;5;252m [39m[38;5;252m?[39m[38;5;252m [39m[38;5;214m"A"[39m[38;5;252m [39m[38;5;252m:[39m[38;5;252m [39m[38;5;214m"B"[39m[38;5;252m;[39m
[38;5;252m        [39m[38;5;252mSystem[39m[38;5;252m.[39m[38;5;250mout[39m[38;5;252m.[39m[38;5;250mprintln[39m[38;5;252m([39m[38;5;214m"bar : "[39m[38;5;252m [39m[38;5;252m+[39m[38;5;252m [39m[38;5;252mbar[39m[38;5;252m)[39m[38;5;252m;[39m[38;5;252m [39m[38;5;246;03m// Prints "bar : A", because the[39;00m
[38;5;252m        [39m[38;5;246;03m// statement is true.[39;00m
[38;5;252m        [39m[38;5;246;03m// Or simply[39;00m
[38;5;252m        [39m[38;5;252mSystem[39m[38;5;252m.[39m[38;5;250mout[39m[38;5;252m.[39m[38;5;250mprintln[39m[38;5;252m([39m[38;5;214m"bar : "[39m[38;5;252m [39m[38;5;252m+[39m[38;5;252m [39m[38;5;252m([39m[38;5;252mfoo[39m[38;5;252m [39m[38;5;252m<[39m[38;5;252m [39m[38;5;67m10[39m[38;5;252m [39m[38;5;252m?[39m[38;5;252m [39m[38;5;214m"A"[39m[38;5;252m [39m[38;5;252m:[39m[38;5;252m [39m[38;5;214m"B"[39m[38;5;252m)[39m[38;5;252m)[39m[38;5;252m;[39m


[38;5;252m        [39m[38;5;246;03m////////////////////////////////////////[39;00m
[38;5;252m        [39m[38;5;246;03m// Converting Data Types[39;00m
[38;5;252m        [39m[38;5;246;03m////////////////////////////////////////[39;00m

[38;5;252m        [39m[38;5;246;03m// Converting data[39;00m

[38;5;252m        [39m[38;5;246;03m// Convert String To Integer[39;00m
[38;5;252m        [39m[38;5;252mInteger[39m[38;5;252m.[39m[38;5;250mparseInt[39m[38;5;252m([39m[38;5;214m"123"[39m[38;5;252m)[39m[38;5;252m;[39m[38;5;246;03m//returns an integer version of "123"[39;00m

[38;5;252m        [39m[38;5;246;03m// Convert Integer To String[39;00m
[38;5;252m        [39m[38;5;252mInteger[39m[38;5;252m.[39m[38;5;250mtoString[39m[38;5;252m([39m[38;5;67m123[39m[38;5;252m)[39m[38;5;252m;[39m[38;5;246;03m//returns a string version of 123[39;00m

[38;5;252m        [39m[38;5;246;03m// For other conversions check out the following classes:[39;00m
[38;5;252m        [39m[38;5;246;03m// Double[39;00m
[38;5;252m        [39m[38;5;246;03m// Long[39;00m
[38;5;252m        [39m[38;5;246;03m// String[39;00m

[38;5;252m        [39m[38;5;246;03m///////////////////////////////////////[39;00m
[38;5;252m        [39m[38;5;246;03m// Classes And Functions[39;00m
[38;5;252m        [39m[38;5;246;03m///////////////////////////////////////[39;00m

[38;5;252m        [39m[38;5;252mSystem[39m[38;5;252m.[39m[38;5;250mout[39m[38;5;252m.[39m[38;5;250mprintln[39m[38;5;252m([39m[38;5;214m"\n->Classes & Functions"[39m[38;5;252m)[39m[38;5;252m;[39m

[38;5;252m        [39m[38;5;246;03m// (definition of the Bicycle class follows)[39;00m

[38;5;252m        [39m[38;5;246;03m// Use new to instantiate a class[39;00m
[38;5;252m        [39m[38;5;252mBicycle[39m[38;5;252m [39m[38;5;252mtrek[39m[38;5;252m [39m[38;5;252m=[39m[38;5;252m [39m[38;5;70;01mnew[39;00m[38;5;252m [39m[38;5;252mBicycle[39m[38;5;252m([39m[38;5;252m)[39m[38;5;252m;[39m

[38;5;252m        [39m[38;5;246;03m// Call object methods[39;00m
[38;5;252m        [39m[38;5;252mtrek[39m[38;5;252m.[39m[38;5;250mspeedUp[39m[38;5;252m([39m[38;5;67m3[39m[38;5;252m)[39m[38;5;252m;[39m[38;5;252m [39m[38;5;246;03m// You should always use setter and getter methods[39;00m
[38;5;252m        [39m[38;5;252mtrek[39m[38;5;252m.[39m[38;5;250msetCadence[39m[38;5;252m([39m[38;5;67m100[39m[38;5;252m)[39m[38;5;252m;[39m

[38;5;252m        [39m[38;5;246;03m// toString returns this Object's string representation.[39;00m
[38;5;252m        [39m[38;5;252mSystem[39m[38;5;252m.[39m[38;5;250mout[39m[38;5;252m.[39m[38;5;250mprintln[39m[38;5;252m([39m[38;5;214m"trek info: "[39m[38;5;252m [39m[38;5;252m+[39m[38;5;252m [39m[38;5;252mtrek[39m[38;5;252m.[39m[38;5;250mtoString[39m[38;5;252m([39m[38;5;252m)[39m[38;5;252m)[39m[38;5;252m;[39m
[38;5;252m    [39m[38;5;252m}[39m[38;5;252m [39m[38;5;246;03m// End main method[39;00m

[38;5;252m    [39m[38;5;70;01mprivate[39;00m[38;5;252m [39m[38;5;70;01mstatic[39;00m[38;5;252m [39m[38;5;70;01mclass[39;00m[38;5;252m [39m[38;5;68;04mTestInitialization[39;00m[38;5;252m [39m[38;5;252m{[39m
[38;5;252m        [39m[38;5;246;03m// Double Brace Initialization[39;00m
[38;5;252m        [39m[38;5;246;03m// Before Java 11, the Java Language had no syntax for how to create[39;00m
[38;5;252m        [39m[38;5;246;03m// static Collections in an easy way. Usually you end up like this:[39;00m
[38;5;252m        [39m[38;5;70;01mprivate[39;00m[38;5;252m [39m[38;5;70;01mstatic[39;00m[38;5;252m [39m[38;5;70;01mfinal[39;00m[38;5;252m [39m[38;5;252mSet[39m[38;5;252m<[39m[38;5;252mString[39m[38;5;252m>[39m[38;5;252m [39m[38;5;252mCOUNTRIES[39m[38;5;252m [39m[38;5;252m=[39m[38;5;252m [39m[38;5;70;01mnew[39;00m[38;5;252m [39m[38;5;252mHashSet[39m[38;5;252m<[39m[38;5;252mString[39m[38;5;252m>[39m[38;5;252m([39m[38;5;252m)[39m[38;5;252m;[39m
[38;5;252m        [39m[38;5;70;01mstatic[39;00m[38;5;252m [39m[38;5;252m{[39m
[38;5;252m           [39m[38;5;252mCOUNTRIES[39m[38;5;252m.[39m[38;5;250madd[39m[38;5;252m([39m[38;5;214m"DENMARK"[39m[38;5;252m)[39m[38;5;252m;[39m
[38;5;252m           [39m[38;5;252mCOUNTRIES[39m[38;5;252m.[39m[38;5;250madd[39m[38;5;252m([39m[38;5;214m"SWEDEN"[39m[38;5;252m)[39m[38;5;252m;[39m
[38;5;252m           [39m[38;5;252mCOUNTRIES[39m[38;5;252m.[39m[38;5;250madd[39m[38;5;252m([39m[38;5;214m"FINLAND"[39m[38;5;252m)[39m[38;5;252m;[39m
[38;5;252m        [39m[38;5;252m}[39m

[38;5;252m        [39m[38;5;246;03m// There's a nifty way to achieve the same thing, [39;00m
[38;5;252m        [39m[38;5;246;03m// by using something that is called Double Brace Initialization.[39;00m
[38;5;252m        [39m[38;5;70;01mprivate[39;00m[38;5;252m [39m[38;5;70;01mstatic[39;00m[38;5;252m [39m[38;5;70;01mfinal[39;00m[38;5;252m [39m[38;5;252mSet[39m[38;5;252m<[39m[38;5;252mString[39m[38;5;252m>[39m[38;5;252m [39m[38;5;252mCOUNTRIES_DOUBLE_BRACE[39m[38;5;252m [39m[38;5;252m=[39m[38;5;252m [39m
[38;5;252m        [39m[38;5;70;01mnew[39;00m[38;5;252m [39m[38;5;252mHashSet[39m[38;5;252m<[39m[38;5;252mString[39m[38;5;252m>[39m[38;5;252m([39m[38;5;252m)[39m[38;5;252m [39m[38;5;252m{[39m[38;5;252m{[39m
[38;5;252m            [39m[38;5;252madd[39m[38;5;252m([39m[38;5;214m"DENMARK"[39m[38;5;252m)[39m[38;5;252m;[39m
[38;5;252m            [39m[38;5;252madd[39m[38;5;252m([39m[38;5;214m"SWEDEN"[39m[38;5;252m)[39m[38;5;252m;[39m
[38;5;252m            [39m[38;5;252madd[39m[38;5;252m([39m[38;5;214m"FINLAND"[39m[38;5;252m)[39m[38;5;252m;[39m
[38;5;252m        [39m[38;5;252m}[39m[38;5;252m}[39m

[38;5;252m        [39m[38;5;246;03m// The first brace is creating a new AnonymousInnerClass and the[39;00m
[38;5;252m        [39m[38;5;246;03m// second one declares an instance initializer block. This block[39;00m
[38;5;252m        [39m[38;5;246;03m// is called when the anonymous inner class is created.[39;00m
[38;5;252m        [39m[38;5;246;03m// This does not only work for Collections, it works for all[39;00m
[38;5;252m        [39m[38;5;246;03m// non-final classes.[39;00m
[38;5;252m        [39m

[38;5;252m        [39m[38;5;246;03m// Another option was to initialize the Collection from an array,[39;00m
[38;5;252m        [39m[38;5;246;03m// using Arrays.asList() method:[39;00m
[38;5;252m        [39m[38;5;70;01mprivate[39;00m[38;5;252m [39m[38;5;70;01mstatic[39;00m[38;5;252m [39m[38;5;70;01mfinal[39;00m[38;5;252m [39m[38;5;252mList[39m[38;5;252m<[39m[38;5;252mString[39m[38;5;252m>[39m[38;5;252m [39m[38;5;252mCOUNTRIES_AS_LIST[39m[38;5;252m [39m[38;5;252m=[39m[38;5;252m [39m
[38;5;252m                        [39m[38;5;252mArrays[39m[38;5;252m.[39m[38;5;250masList[39m[38;5;252m([39m[38;5;214m"SWEDEN"[39m[38;5;252m,[39m[38;5;252m [39m[38;5;214m"DENMARK"[39m[38;5;252m,[39m[38;5;252m [39m[38;5;214m"NORWAY"[39m[38;5;252m)[39m[38;5;252m;[39m
[38;5;252m        [39m[38;5;246;03m// This has one catch: the list we get is internally backed by the array,[39;00m
[38;5;252m        [39m[38;5;246;03m// and since arrays can't change their size, the list backed by the array[39;00m
[38;5;252m        [39m[38;5;246;03m// is not resizeable, which means we can't add new elements to it: [39;00m
[38;5;252m        [39m[38;5;70;01mpublic[39;00m[38;5;252m [39m[38;5;70;01mstatic[39;00m[38;5;252m [39m[38;5;70;01mvoid[39;00m[38;5;252m [39m[38;5;68mmain[39m[38;5;252m([39m[38;5;252mString[39m[38;5;252m[[39m[38;5;252m][39m[38;5;252m [39m[38;5;252margs[39m[38;5;252m)[39m[38;5;252m [39m[38;5;252m{[39m
[38;5;252m            [39m[38;5;252mCOUNTRIES[39m[38;5;252m.[39m[38;5;250madd[39m[38;5;252m([39m[38;5;214m"FINLAND"[39m[38;5;252m)[39m[38;5;252m;[39m[38;5;252m [39m[38;5;246;03m// throws UnsupportedOperationException![39;00m
[38;5;252m            [39m[38;5;246;03m// However, we can replace elements by index, just like in array: [39;00m
[38;5;252m            [39m[38;5;252mCOUNTRIES[39m[38;5;252m.[39m[38;5;250mset[39m[38;5;252m([39m[38;5;67m1[39m[38;5;252m,[39m[38;5;252m [39m[38;5;214m"FINLAND"[39m[38;5;252m)[39m[38;5;252m;[39m
[38;5;252m            [39m[38;5;252mSystem[39m[38;5;252m.[39m[38;5;250mout[39m[38;5;252m.[39m[38;5;250mprintln[39m[38;5;252m([39m[38;5;252mCOUNTRIES[39m[38;5;252m)[39m[38;5;252m;[39m[38;5;252m [39m[38;5;246;03m// prints [SWEDEN, FINLAND, NORWAY][39;00m
[38;5;252m        [39m[38;5;252m}[39m
[38;5;252m        [39m[38;5;246;03m// The resizing problem can be circumvented [39;00m
[38;5;252m        [39m[38;5;246;03m// by creating another Collection from the List:[39;00m
[38;5;252m         [39m[38;5;70;01mprivate[39;00m[38;5;252m [39m[38;5;70;01mstatic[39;00m[38;5;252m [39m[38;5;70;01mfinal[39;00m[38;5;252m [39m[38;5;252mSet[39m[38;5;252m<[39m[38;5;252mString[39m[38;5;252m>[39m[38;5;252m [39m[38;5;252mCOUNTRIES_SET[39m[38;5;252m [39m[38;5;252m=[39m[38;5;252m [39m
[38;5;252m                [39m[38;5;70;01mnew[39;00m[38;5;252m [39m[38;5;252mHashSet[39m[38;5;252m<[39m[38;5;252m>[39m[38;5;252m([39m[38;5;252mArrays[39m[38;5;252m.[39m[38;5;250masList[39m[38;5;252m([39m[38;5;214m"SWEDEN"[39m[38;5;252m,[39m[38;5;252m [39m[38;5;214m"DENMARK"[39m[38;5;252m,[39m[38;5;252m [39m[38;5;214m"NORWAY"[39m[38;5;252m)[39m[38;5;252m)[39m[38;5;252m;[39m
[38;5;252m        [39m[38;5;246;03m// It's perfectly fine to add anything to the Set of COUNTRIES now. [39;00m
[38;5;252m    [39m[38;5;252m}[39m[38;5;252m [39m[38;5;246;03m// End TestInitialization class[39;00m

[38;5;252m    [39m[38;5;70;01mprivate[39;00m[38;5;252m [39m[38;5;70;01mstatic[39;00m[38;5;252m [39m[38;5;70;01mclass[39;00m[38;5;252m [39m[38;5;68;04mTestJava11Initialization[39;00m[38;5;252m [39m[38;5;252m{[39m
[38;5;252m        [39m[38;5;246;03m// Since Java 11, there is a convenient option to initialize Collections:[39;00m
[38;5;252m        [39m[38;5;246;03m// Set.of() and List.of() methods. [39;00m
[38;5;252m        [39m[38;5;70;01mprivate[39;00m[38;5;252m [39m[38;5;70;01mstatic[39;00m[38;5;252m [39m[38;5;70;01mfinal[39;00m[38;5;252m [39m[38;5;252mSet[39m[38;5;252m<[39m[38;5;252mString[39m[38;5;252m>[39m[38;5;252m [39m[38;5;252mCOUNTRIES[39m[38;5;252m [39m[38;5;252m=[39m[38;5;252m [39m
[38;5;252m                [39m[38;5;252mSet[39m[38;5;252m.[39m[38;5;250mof[39m[38;5;252m([39m[38;5;214m"SWEDEN"[39m[38;5;252m,[39m[38;5;252m [39m[38;5;214m"DENMARK"[39m[38;5;252m,[39m[38;5;252m [39m[38;5;214m"NORWAY"[39m[38;5;252m)[39m[38;5;252m;[39m
[38;5;252m        [39m[38;5;246;03m// There is a massive catch, though: Lists and Sets initialized like this [39;00m
[38;5;252m        [39m[38;5;246;03m// 1) are immutable [39;00m
[38;5;252m        [39m[38;5;246;03m// 2) can't contain null elements (even check for null elements fails)![39;00m
[38;5;252m        [39m[38;5;70;01mpublic[39;00m[38;5;252m [39m[38;5;70;01mstatic[39;00m[38;5;252m [39m[38;5;70;01mvoid[39;00m[38;5;252m [39m[38;5;68mmain[39m[38;5;252m([39m[38;5;252mString[39m[38;5;252m[[39m[38;5;252m][39m[38;5;252m [39m[38;5;252margs[39m[38;5;252m)[39m[38;5;252m [39m[38;5;252m{[39m
[38;5;252m            [39m[38;5;252mCOUNTRIES[39m[38;5;252m.[39m[38;5;250madd[39m[38;5;252m([39m[38;5;214m"FINLAND"[39m[38;5;252m)[39m[38;5;252m;[39m[38;5;252m [39m[38;5;246;03m// throws UnsupportedOperationException[39;00m
[38;5;252m            [39m[38;5;252mCOUNTRIES[39m[38;5;252m.[39m[38;5;250mremove[39m[38;5;252m([39m[38;5;214m"NORWAY"[39m[38;5;252m)[39m[38;5;252m;[39m[38;5;252m [39m[38;5;246;03m// throws UnsupportedOperationException [39;00m
[38;5;252m            [39m[38;5;252mCOUNTRIES[39m[38;5;252m.[39m[38;5;250mcontains[39m[38;5;252m([39m[38;5;70;01mnull[39;00m[38;5;252m)[39m[38;5;252m;[39m[38;5;252m [39m[38;5;246;03m// throws NullPointerException[39;00m
[38;5;252m        [39m[38;5;252m}[39m
[38;5;252m        [39m[38;5;70;01mprivate[39;00m[38;5;252m [39m[38;5;70;01mstatic[39;00m[38;5;252m [39m[38;5;70;01mfinal[39;00m[38;5;252m [39m[38;5;252mSet[39m[38;5;252m<[39m[38;5;252mString[39m[38;5;252m>[39m[38;5;252m [39m[38;5;252mCOUNTRIES_WITH_NULL[39m[38;5;252m [39m[38;5;252m=[39m[38;5;252m [39m
[38;5;252m                    [39m[38;5;252mSet[39m[38;5;252m.[39m[38;5;250mof[39m[38;5;252m([39m[38;5;214m"SWEDEN"[39m[38;5;252m,[39m[38;5;252m [39m[38;5;70;01mnull[39;00m[38;5;252m,[39m[38;5;252m [39m[38;5;214m"NORWAY"[39m[38;5;252m)[39m[38;5;252m;[39m[38;5;252m [39m[38;5;246;03m// throws NullPointerException[39;00m

[38;5;252m    [39m[38;5;252m}[39m[38;5;252m [39m[38;5;246;03m// End TestJava11Initialization class[39;00m
[38;5;252m}[39m[38;5;252m [39m[38;5;246;03m// End LearnJava class[39;00m

[38;5;246;03m// You can include other, non-public outer-level classes in a .java file,[39;00m
[38;5;246;03m// but it is not good practice. Instead split classes into separate files.[39;00m

[38;5;246;03m// Class Declaration Syntax:[39;00m
[38;5;246;03m// <public/private/protected> class <class name> {[39;00m
[38;5;246;03m//    // data fields, constructors, functions all inside.[39;00m
[38;5;246;03m//    // functions are called as methods in Java.[39;00m
[38;5;246;03m// }[39;00m

[38;5;70;01mclass[39;00m[38;5;252m [39m[38;5;68;04mBicycle[39;00m[38;5;252m [39m[38;5;252m{[39m

[38;5;252m    [39m[38;5;246;03m// Bicycle's Fields/Variables[39;00m
[38;5;252m    [39m[38;5;70;01mpublic[39;00m[38;5;252m [39m[38;5;70;01mint[39;00m[38;5;252m [39m[38;5;252mcadence[39m[38;5;252m;[39m[38;5;252m [39m[38;5;246;03m// Public: Can be accessed from anywhere[39;00m
[38;5;252m    [39m[38;5;70;01mprivate[39;00m[38;5;252m [39m[38;5;70;01mint[39;00m[38;5;252m [39m[38;5;252mspeed[39m[38;5;252m;[39m[38;5;252m  [39m[38;5;246;03m// Private: Only accessible from within the class[39;00m
[38;5;252m    [39m[38;5;70;01mprotected[39;00m[38;5;252m [39m[38;5;70;01mint[39;00m[38;5;252m [39m[38;5;252mgear[39m[38;5;252m;[39m[38;5;252m [39m[38;5;246;03m// Protected: Accessible from the class and subclasses[39;00m
[38;5;252m    [39m[38;5;252mString[39m[38;5;252m [39m[38;5;252mname[39m[38;5;252m;[39m[38;5;252m [39m[38;5;246;03m// default: Only accessible from within this package[39;00m
[38;5;252m    [39m[38;5;70;01mstatic[39;00m[38;5;252m [39m[38;5;252mString[39m[38;5;252m [39m[38;5;252mclassName[39m[38;5;252m;[39m[38;5;252m [39m[38;5;246;03m// Static class variable[39;00m

[38;5;252m    [39m[38;5;246;03m// Static block[39;00m
[38;5;252m    [39m[38;5;246;03m// Java has no implementation of static constructors, but[39;00m
[38;5;252m    [39m[38;5;246;03m// has a static block that can be used to initialize class variables[39;00m
[38;5;252m    [39m[38;5;246;03m// (static variables).[39;00m
[38;5;252m    [39m[38;5;246;03m// This block will be called when the class is loaded.[39;00m
[38;5;252m    [39m[38;5;70;01mstatic[39;00m[38;5;252m [39m[38;5;252m{[39m
[38;5;252m        [39m[38;5;252mclassName[39m[38;5;252m [39m[38;5;252m=[39m[38;5;252m [39m[38;5;214m"Bicycle"[39m[38;5;252m;[39m
[38;5;252m    [39m[38;5;252m}[39m

[38;5;252m    [39m[38;5;246;03m// Constructors are a way of creating classes[39;00m
[38;5;252m    [39m[38;5;246;03m// This is a constructor[39;00m
[38;5;252m    [39m[38;5;70;01mpublic[39;00m[38;5;252m [39m[38;5;68mBicycle[39m[38;5;252m([39m[38;5;252m)[39m[38;5;252m [39m[38;5;252m{[39m
[38;5;252m        [39m[38;5;246;03m// You can also call another constructor:[39;00m
[38;5;252m        [39m[38;5;246;03m// this(1, 50, 5, "Bontrager");[39;00m
[38;5;252m        [39m[38;5;252mgear[39m[38;5;252m [39m[38;5;252m=[39m[38;5;252m [39m[38;5;67m1[39m[38;5;252m;[39m
[38;5;252m        [39m[38;5;252mcadence[39m[38;5;252m [39m[38;5;252m=[39m[38;5;252m [39m[38;5;67m50[39m[38;5;252m;[39m
[38;5;252m        [39m[38;5;252mspeed[39m[38;5;252m [39m[38;5;252m=[39m[38;5;252m [39m[38;5;67m5[39m[38;5;252m;[39m
[38;5;252m        [39m[38;5;252mname[39m[38;5;252m [39m[38;5;252m=[39m[38;5;252m [39m[38;5;214m"Bontrager"[39m[38;5;252m;[39m
[38;5;252m    [39m[38;5;252m}[39m
[38;5;252m    [39m[38;5;246;03m// This is a constructor that takes arguments[39;00m
[38;5;252m    [39m[38;5;70;01mpublic[39;00m[38;5;252m [39m[38;5;68mBicycle[39m[38;5;252m([39m[38;5;70;01mint[39;00m[38;5;252m [39m[38;5;252mstartCadence[39m[38;5;252m,[39m[38;5;252m [39m[38;5;70;01mint[39;00m[38;5;252m [39m[38;5;252mstartSpeed[39m[38;5;252m,[39m[38;5;252m [39m[38;5;70;01mint[39;00m[38;5;252m [39m[38;5;252mstartGear[39m[38;5;252m,[39m
[38;5;252m        [39m[38;5;252mString[39m[38;5;252m [39m[38;5;252mname[39m[38;5;252m)[39m[38;5;252m [39m[38;5;252m{[39m
[38;5;252m        [39m[38;5;70;01mthis[39;00m[38;5;252m.[39m[38;5;250mgear[39m[38;5;252m [39m[38;5;252m=[39m[38;5;252m [39m[38;5;252mstartGear[39m[38;5;252m;[39m
[38;5;252m        [39m[38;5;70;01mthis[39;00m[38;5;252m.[39m[38;5;250mcadence[39m[38;5;252m [39m[38;5;252m=[39m[38;5;252m [39m[38;5;252mstartCadence[39m[38;5;252m;[39m
[38;5;252m        [39m[38;5;70;01mthis[39;00m[38;5;252m.[39m[38;5;250mspeed[39m[38;5;252m [39m[38;5;252m=[39m[38;5;252m [39m[38;5;252mstartSpeed[39m[38;5;252m;[39m
[38;5;252m        [39m[38;5;70;01mthis[39;00m[38;5;252m.[39m[38;5;250mname[39m[38;5;252m [39m[38;5;252m=[39m[38;5;252m [39m[38;5;252mname[39m[38;5;252m;[39m
[38;5;252m    [39m[38;5;252m}[39m

[38;5;252m    [39m[38;5;246;03m// Method Syntax:[39;00m
[38;5;252m    [39m[38;5;246;03m// <public/private/protected> <return type> <function name>(<args>)[39;00m

[38;5;252m    [39m[38;5;246;03m// Java classes often implement getters and setters for their fields[39;00m

[38;5;252m    [39m[38;5;246;03m// Method declaration syntax:[39;00m
[38;5;252m    [39m[38;5;246;03m// <access modifier> <return type> <method name>(<args>)[39;00m
[38;5;252m    [39m[38;5;70;01mpublic[39;00m[38;5;252m [39m[38;5;70;01mint[39;00m[38;5;252m [39m[38;5;68mgetCadence[39m[38;5;252m([39m[38;5;252m)[39m[38;5;252m [39m[38;5;252m{[39m
[38;5;252m        [39m[38;5;70;01mreturn[39;00m[38;5;252m [39m[38;5;252mcadence[39m[38;5;252m;[39m
[38;5;252m    [39m[38;5;252m}[39m

[38;5;252m    [39m[38;5;246;03m// void methods require no return statement[39;00m
[38;5;252m    [39m[38;5;70;01mpublic[39;00m[38;5;252m [39m[38;5;70;01mvoid[39;00m[38;5;252m [39m[38;5;68msetCadence[39m[38;5;252m([39m[38;5;70;01mint[39;00m[38;5;252m [39m[38;5;252mnewValue[39m[38;5;252m)[39m[38;5;252m [39m[38;5;252m{[39m
[38;5;252m        [39m[38;5;252mcadence[39m[38;5;252m [39m[38;5;252m=[39m[38;5;252m [39m[38;5;252mnewValue[39m[38;5;252m;[39m
[38;5;252m    [39m[38;5;252m}[39m
[38;5;252m    [39m[38;5;70;01mpublic[39;00m[38;5;252m [39m[38;5;70;01mvoid[39;00m[38;5;252m [39m[38;5;68msetGear[39m[38;5;252m([39m[38;5;70;01mint[39;00m[38;5;252m [39m[38;5;252mnewValue[39m[38;5;252m)[39m[38;5;252m [39m[38;5;252m{[39m
[38;5;252m        [39m[38;5;252mgear[39m[38;5;252m [39m[38;5;252m=[39m[38;5;252m [39m[38;5;252mnewValue[39m[38;5;252m;[39m
[38;5;252m    [39m[38;5;252m}[39m
[38;5;252m    [39m[38;5;70;01mpublic[39;00m[38;5;252m [39m[38;5;70;01mvoid[39;00m[38;5;252m [39m[38;5;68mspeedUp[39m[38;5;252m([39m[38;5;70;01mint[39;00m[38;5;252m [39m[38;5;252mincrement[39m[38;5;252m)[39m[38;5;252m [39m[38;5;252m{[39m
[38;5;252m        [39m[38;5;252mspeed[39m[38;5;252m [39m[38;5;252m+[39m[38;5;252m=[39m[38;5;252m [39m[38;5;252mincrement[39m[38;5;252m;[39m
[38;5;252m    [39m[38;5;252m}[39m
[38;5;252m    [39m[38;5;70;01mpublic[39;00m[38;5;252m [39m[38;5;70;01mvoid[39;00m[38;5;252m [39m[38;5;68mslowDown[39m[38;5;252m([39m[38;5;70;01mint[39;00m[38;5;252m [39m[38;5;252mdecrement[39m[38;5;252m)[39m[38;5;252m [39m[38;5;252m{[39m
[38;5;252m        [39m[38;5;252mspeed[39m[38;5;252m [39m[38;5;252m-[39m[38;5;252m=[39m[38;5;252m [39m[38;5;252mdecrement[39m[38;5;252m;[39m
[38;5;252m    [39m[38;5;252m}[39m
[38;5;252m    [39m[38;5;70;01mpublic[39;00m[38;5;252m [39m[38;5;70;01mvoid[39;00m[38;5;252m [39m[38;5;68msetName[39m[38;5;252m([39m[38;5;252mString[39m[38;5;252m [39m[38;5;252mnewName[39m[38;5;252m)[39m[38;5;252m [39m[38;5;252m{[39m
[38;5;252m        [39m[38;5;252mname[39m[38;5;252m [39m[38;5;252m=[39m[38;5;252m [39m[38;5;252mnewName[39m[38;5;252m;[39m
[38;5;252m    [39m[38;5;252m}[39m
[38;5;252m    [39m[38;5;70;01mpublic[39;00m[38;5;252m [39m[38;5;252mString[39m[38;5;252m [39m[38;5;68mgetName[39m[38;5;252m([39m[38;5;252m)[39m[38;5;252m [39m[38;5;252m{[39m
[38;5;252m        [39m[38;5;70;01mreturn[39;00m[38;5;252m [39m[38;5;252mname[39m[38;5;252m;[39m
[38;5;252m    [39m[38;5;252m}[39m

[38;5;252m    [39m[38;5;246;03m//Method to display the attribute values of this Object.[39;00m
[38;5;252m    [39m[38;5;214m@Override[39m[38;5;252m [39m[38;5;246;03m// Inherited from the Object class.[39;00m
[38;5;252m    [39m[38;5;70;01mpublic[39;00m[38;5;252m [39m[38;5;252mString[39m[38;5;252m [39m[38;5;68mtoString[39m[38;5;252m([39m[38;5;252m)[39m[38;5;252m [39m[38;5;252m{[39m
[38;5;252m        [39m[38;5;70;01mreturn[39;00m[38;5;252m [39m[38;5;214m"gear: "[39m[38;5;252m [39m[38;5;252m+[39m[38;5;252m [39m[38;5;252mgear[39m[38;5;252m [39m[38;5;252m+[39m[38;5;252m [39m[38;5;214m" cadence: "[39m[38;5;252m [39m[38;5;252m+[39m[38;5;252m [39m[38;5;252mcadence[39m[38;5;252m [39m[38;5;252m+[39m[38;5;252m [39m[38;5;214m" speed: "[39m[38;5;252m [39m[38;5;252m+[39m[38;5;252m [39m[38;5;252mspeed[39m[38;5;252m [39m[38;5;252m+[39m
[38;5;252m            [39m[38;5;214m" name: "[39m[38;5;252m [39m[38;5;252m+[39m[38;5;252m [39m[38;5;252mname[39m[38;5;252m;[39m
[38;5;252m    [39m[38;5;252m}[39m
[38;5;252m}[39m[38;5;252m [39m[38;5;246;03m// end class Bicycle[39;00m

[38;5;246;03m// PennyFarthing is a subclass of Bicycle[39;00m
[38;5;70;01mclass[39;00m[38;5;252m [39m[38;5;68;04mPennyFarthing[39;00m[38;5;252m [39m[38;5;70;01mextends[39;00m[38;5;252m [39m[38;5;252mBicycle[39m[38;5;252m [39m[38;5;252m{[39m
[38;5;252m    [39m[38;5;246;03m// (Penny Farthings are those bicycles with the big front wheel.[39;00m
[38;5;252m    [39m[38;5;246;03m// They have no gears.)[39;00m

[38;5;252m    [39m[38;5;70;01mpublic[39;00m[38;5;252m [39m[38;5;68mPennyFarthing[39m[38;5;252m([39m[38;5;70;01mint[39;00m[38;5;252m [39m[38;5;252mstartCadence[39m[38;5;252m,[39m[38;5;252m [39m[38;5;70;01mint[39;00m[38;5;252m [39m[38;5;252mstartSpeed[39m[38;5;252m)[39m[38;5;252m [39m[38;5;252m{[39m
[38;5;252m        [39m[38;5;246;03m// Call the parent constructor with super[39;00m
[38;5;252m        [39m[38;5;70;01msuper[39;00m[38;5;252m([39m[38;5;252mstartCadence[39m[38;5;252m,[39m[38;5;252m [39m[38;5;252mstartSpeed[39m[38;5;252m,[39m[38;5;252m [39m[38;5;67m0[39m[38;5;252m,[39m[38;5;252m [39m[38;5;214m"PennyFarthing"[39m[38;5;252m)[39m[38;5;252m;[39m
[38;5;252m    [39m[38;5;252m}[39m

[38;5;252m    [39m[38;5;246;03m// You should mark a method you're overriding with an @annotation.[39;00m
[38;5;252m    [39m[38;5;246;03m// To learn more about what annotations are and their purpose check this[39;00m
[38;5;252m    [39m[38;5;246;03m// out: http://docs.oracle.com/javase/tutorial/java/annotations/[39;00m
[38;5;252m    [39m[38;5;214m@Override[39m
[38;5;252m    [39m[38;5;70;01mpublic[39;00m[38;5;252m [39m[38;5;70;01mvoid[39;00m[38;5;252m [39m[38;5;68msetGear[39m[38;5;252m([39m[38;5;70;01mint[39;00m[38;5;252m [39m[38;5;252mgear[39m[38;5;252m)[39m[38;5;252m [39m[38;5;252m{[39m
[38;5;252m        [39m[38;5;70;01mthis[39;00m[38;5;252m.[39m[38;5;250mgear[39m[38;5;252m [39m[38;5;252m=[39m[38;5;252m [39m[38;5;67m0[39m[38;5;252m;[39m
[38;5;252m    [39m[38;5;252m}[39m
[38;5;252m}[39m

[38;5;246;03m// Object casting[39;00m
[38;5;246;03m// Since the PennyFarthing class is extending the Bicycle class, we can say[39;00m
[38;5;246;03m// a PennyFarthing is a Bicycle and write :[39;00m
[38;5;246;03m// Bicycle bicycle = new PennyFarthing();[39;00m
[38;5;246;03m// This is called object casting where an object is taken for another one. There[39;00m
[38;5;246;03m// are lots of details and deals with some more intermediate concepts here:[39;00m
[38;5;246;03m// https://docs.oracle.com/javase/tutorial/java/IandI/subclasses.html[39;00m

[38;5;246;03m// Interfaces[39;00m
[38;5;246;03m// Interface declaration syntax[39;00m
[38;5;246;03m// <access-level> interface <interface-name> extends <super-interfaces> {[39;00m
[38;5;246;03m//     // Constants[39;00m
[38;5;246;03m//     // Method declarations[39;00m
[38;5;246;03m// }[39;00m

[38;5;246;03m// Example - Food:[39;00m
[38;5;70;01mpublic[39;00m[38;5;252m [39m[38;5;70;01minterface[39;00m[38;5;252m [39m[38;5;68;04mEdible[39;00m[38;5;252m [39m[38;5;252m{[39m
[38;5;252m    [39m[38;5;70;01mpublic[39;00m[38;5;252m [39m[38;5;70;01mvoid[39;00m[38;5;252m [39m[38;5;68meat[39m[38;5;252m([39m[38;5;252m)[39m[38;5;252m;[39m[38;5;252m [39m[38;5;246;03m// Any class that implements this interface, must[39;00m
[38;5;252m                       [39m[38;5;246;03m// implement this method.[39;00m
[38;5;252m}[39m

[38;5;70;01mpublic[39;00m[38;5;252m [39m[38;5;70;01minterface[39;00m[38;5;252m [39m[38;5;68;04mDigestible[39;00m[38;5;252m [39m[38;5;252m{[39m
[38;5;252m    [39m[38;5;70;01mpublic[39;00m[38;5;252m [39m[38;5;70;01mvoid[39;00m[38;5;252m [39m[38;5;68mdigest[39m[38;5;252m([39m[38;5;252m)[39m[38;5;252m;[39m
[38;5;252m    [39m[38;5;246;03m// Since Java 8, interfaces can have default method.[39;00m
[38;5;252m    [39m[38;5;70;01mpublic[39;00m[38;5;252m [39m[38;5;70;01mdefault[39;00m[38;5;252m [39m[38;5;70;01mvoid[39;00m[38;5;252m [39m[38;5;68mdefaultMethod[39m[38;5;252m([39m[38;5;252m)[39m[38;5;252m [39m[38;5;252m{[39m
[38;5;252m        [39m[38;5;252mSystem[39m[38;5;252m.[39m[38;5;250mout[39m[38;5;252m.[39m[38;5;250mprintln[39m[38;5;252m([39m[38;5;214m"Hi from default method ..."[39m[38;5;252m)[39m[38;5;252m;[39m
[38;5;252m    [39m[38;5;252m}[39m
[38;5;252m}[39m

[38;5;246;03m// We can now create a class that implements both of these interfaces.[39;00m
[38;5;70;01mpublic[39;00m[38;5;252m [39m[38;5;70;01mclass[39;00m[38;5;252m [39m[38;5;68;04mFruit[39;00m[38;5;252m [39m[38;5;70;01mimplements[39;00m[38;5;252m [39m[38;5;252mEdible[39m[38;5;252m,[39m[38;5;252m [39m[38;5;252mDigestible[39m[38;5;252m [39m[38;5;252m{[39m
[38;5;252m    [39m[38;5;214m@Override[39m
[38;5;252m    [39m[38;5;70;01mpublic[39;00m[38;5;252m [39m[38;5;70;01mvoid[39;00m[38;5;252m [39m[38;5;68meat[39m[38;5;252m([39m[38;5;252m)[39m[38;5;252m [39m[38;5;252m{[39m
[38;5;252m        [39m[38;5;246;03m// ...[39;00m
[38;5;252m    [39m[38;5;252m}[39m

[38;5;252m    [39m[38;5;214m@Override[39m
[38;5;252m    [39m[38;5;70;01mpublic[39;00m[38;5;252m [39m[38;5;70;01mvoid[39;00m[38;5;252m [39m[38;5;68mdigest[39m[38;5;252m([39m[38;5;252m)[39m[38;5;252m [39m[38;5;252m{[39m
[38;5;252m        [39m[38;5;246;03m// ...[39;00m
[38;5;252m    [39m[38;5;252m}[39m
[38;5;252m}[39m

[38;5;246;03m// In Java, you can extend only one class, but you can implement many[39;00m
[38;5;246;03m// interfaces. For example:[39;00m
[38;5;70;01mpublic[39;00m[38;5;252m [39m[38;5;70;01mclass[39;00m[38;5;252m [39m[38;5;68;04mExampleClass[39;00m[38;5;252m [39m[38;5;70;01mextends[39;00m[38;5;252m [39m[38;5;252mExampleClassParent[39m[38;5;252m [39m[38;5;70;01mimplements[39;00m[38;5;252m [39m[38;5;252mInterfaceOne[39m[38;5;252m,[39m
[38;5;252m    [39m[38;5;252mInterfaceTwo[39m[38;5;252m [39m[38;5;252m{[39m
[38;5;252m    [39m[38;5;214m@Override[39m
[38;5;252m    [39m[38;5;70;01mpublic[39;00m[38;5;252m [39m[38;5;70;01mvoid[39;00m[38;5;252m [39m[38;5;68mInterfaceOneMethod[39m[38;5;252m([39m[38;5;252m)[39m[38;5;252m [39m[38;5;252m{[39m
[38;5;252m    [39m[38;5;252m}[39m

[38;5;252m    [39m[38;5;214m@Override[39m
[38;5;252m    [39m[38;5;70;01mpublic[39;00m[38;5;252m [39m[38;5;70;01mvoid[39;00m[38;5;252m [39m[38;5;68mInterfaceTwoMethod[39m[38;5;252m([39m[38;5;252m)[39m[38;5;252m [39m[38;5;252m{[39m
[38;5;252m    [39m[38;5;252m}[39m

[38;5;252m}[39m

[38;5;246;03m// Abstract Classes[39;00m

[38;5;246;03m// Abstract Class declaration syntax[39;00m
[38;5;246;03m// <access-level> abstract class <abstract-class-name> extends[39;00m
[38;5;246;03m// <super-abstract-classes> {[39;00m
[38;5;246;03m//     // Constants and variables[39;00m
[38;5;246;03m//     // Method declarations[39;00m
[38;5;246;03m// }[39;00m

[38;5;246;03m// Abstract Classes cannot be instantiated.[39;00m
[38;5;246;03m// Abstract classes may define abstract methods.[39;00m
[38;5;246;03m// Abstract methods have no body and are marked abstract[39;00m
[38;5;246;03m// Non-abstract child classes must @Override all abstract methods[39;00m
[38;5;246;03m// from their super-classes.[39;00m
[38;5;246;03m// Abstract classes can be useful when combining repetitive logic[39;00m
[38;5;246;03m// with customised behavior, but as Abstract classes require[39;00m
[38;5;246;03m// inheritance, they violate "Composition over inheritance"[39;00m
[38;5;246;03m// so consider other approaches using composition.[39;00m
[38;5;246;03m// https://en.wikipedia.org/wiki/Composition_over_inheritance[39;00m

[38;5;70;01mpublic[39;00m[38;5;252m [39m[38;5;70;01mabstract[39;00m[38;5;252m [39m[38;5;70;01mclass[39;00m[38;5;252m [39m[38;5;68;04mAnimal[39;00m
[38;5;252m{[39m
[38;5;252m    [39m[38;5;70;01mprivate[39;00m[38;5;252m [39m[38;5;70;01mint[39;00m[38;5;252m [39m[38;5;252mage[39m[38;5;252m;[39m

[38;5;252m    [39m[38;5;70;01mpublic[39;00m[38;5;252m [39m[38;5;70;01mabstract[39;00m[38;5;252m [39m[38;5;70;01mvoid[39;00m[38;5;252m [39m[38;5;68mmakeSound[39m[38;5;252m([39m[38;5;252m)[39m[38;5;252m;[39m

[38;5;252m    [39m[38;5;246;03m// Method can have a body[39;00m
[38;5;252m    [39m[38;5;70;01mpublic[39;00m[38;5;252m [39m[38;5;70;01mvoid[39;00m[38;5;252m [39m[38;5;68meat[39m[38;5;252m([39m[38;5;252m)[39m
[38;5;252m    [39m[38;5;252m{[39m
[38;5;252m        [39m[38;5;252mSystem[39m[38;5;252m.[39m[38;5;250mout[39m[38;5;252m.[39m[38;5;250mprintln[39m[38;5;252m([39m[38;5;214m"I am an animal and I am Eating."[39m[38;5;252m)[39m[38;5;252m;[39m
[38;5;252m        [39m[38;5;246;03m// Note: We can access private variable here.[39;00m
[38;5;252m        [39m[38;5;252mage[39m[38;5;252m [39m[38;5;252m=[39m[38;5;252m [39m[38;5;67m30[39m[38;5;252m;[39m
[38;5;252m    [39m[38;5;252m}[39m

[38;5;252m    [39m[38;5;70;01mpublic[39;00m[38;5;252m [39m[38;5;70;01mvoid[39;00m[38;5;252m [39m[38;5;68mprintAge[39m[38;5;252m([39m[38;5;252m)[39m
[38;5;252m    [39m[38;5;252m{[39m
[38;5;252m        [39m[38;5;252mSystem[39m[38;5;252m.[39m[38;5;250mout[39m[38;5;252m.[39m[38;5;250mprintln[39m[38;5;252m([39m[38;5;252mage[39m[38;5;252m)[39m[38;5;252m;[39m
[38;5;252m    [39m[38;5;252m}[39m

[38;5;252m    [39m[38;5;246;03m// Abstract classes can have main method.[39;00m
[38;5;252m    [39m[38;5;70;01mpublic[39;00m[38;5;252m [39m[38;5;70;01mstatic[39;00m[38;5;252m [39m[38;5;70;01mvoid[39;00m[38;5;252m [39m[38;5;68mmain[39m[38;5;252m([39m[38;5;252mString[39m[38;5;252m[[39m[38;5;252m][39m[38;5;252m [39m[38;5;252margs[39m[38;5;252m)[39m
[38;5;252m    [39m[38;5;252m{[39m
[38;5;252m        [39m[38;5;252mSystem[39m[38;5;252m.[39m[38;5;250mout[39m[38;5;252m.[39m[38;5;250mprintln[39m[38;5;252m([39m[38;5;214m"I am abstract"[39m[38;5;252m)[39m[38;5;252m;[39m
[38;5;252m    [39m[38;5;252m}[39m
[38;5;252m}[39m

[38;5;70;01mclass[39;00m[38;5;252m [39m[38;5;68;04mDog[39;00m[38;5;252m [39m[38;5;70;01mextends[39;00m[38;5;252m [39m[38;5;252mAnimal[39m
[38;5;252m{[39m
[38;5;252m    [39m[38;5;246;03m// Note still have to override the abstract methods in the[39;00m
[38;5;252m    [39m[38;5;246;03m// abstract class.[39;00m
[38;5;252m    [39m[38;5;214m@Override[39m
[38;5;252m    [39m[38;5;70;01mpublic[39;00m[38;5;252m [39m[38;5;70;01mvoid[39;00m[38;5;252m [39m[38;5;68mmakeSound[39m[38;5;252m([39m[38;5;252m)[39m
[38;5;252m    [39m[38;5;252m{[39m
[38;5;252m        [39m[38;5;252mSystem[39m[38;5;252m.[39m[38;5;250mout[39m[38;5;252m.[39m[38;5;250mprintln[39m[38;5;252m([39m[38;5;214m"Bark"[39m[38;5;252m)[39m[38;5;252m;[39m
[38;5;252m        [39m[38;5;246;03m// age = 30;    ==> ERROR!    age is private to Animal[39;00m
[38;5;252m    [39m[38;5;252m}[39m

[38;5;252m    [39m[38;5;246;03m// NOTE: You will get an error if you used the[39;00m
[38;5;252m    [39m[38;5;246;03m// @Override annotation here, since java doesn't allow[39;00m
[38;5;252m    [39m[38;5;246;03m// overriding of static methods.[39;00m
[38;5;252m    [39m[38;5;246;03m// What is happening here is called METHOD HIDING.[39;00m
[38;5;252m    [39m[38;5;246;03m// Check out this SO post: http://stackoverflow.com/questions/16313649/[39;00m
[38;5;252m    [39m[38;5;70;01mpublic[39;00m[38;5;252m [39m[38;5;70;01mstatic[39;00m[38;5;252m [39m[38;5;70;01mvoid[39;00m[38;5;252m [39m[38;5;68mmain[39m[38;5;252m([39m[38;5;252mString[39m[38;5;252m[[39m[38;5;252m][39m[38;5;252m [39m[38;5;252margs[39m[38;5;252m)[39m
[38;5;252m    [39m[38;5;252m{[39m
[38;5;252m        [39m[38;5;252mDog[39m[38;5;252m [39m[38;5;252mpluto[39m[38;5;252m [39m[38;5;252m=[39m[38;5;252m [39m[38;5;70;01mnew[39;00m[38;5;252m [39m[38;5;252mDog[39m[38;5;252m([39m[38;5;252m)[39m[38;5;252m;[39m
[38;5;252m        [39m[38;5;252mpluto[39m[38;5;252m.[39m[38;5;250mmakeSound[39m[38;5;252m([39m[38;5;252m)[39m[38;5;252m;[39m
[38;5;252m        [39m[38;5;252mpluto[39m[38;5;252m.[39m[38;5;250meat[39m[38;5;252m([39m[38;5;252m)[39m[38;5;252m;[39m
[38;5;252m        [39m[38;5;252mpluto[39m[38;5;252m.[39m[38;5;250mprintAge[39m[38;5;252m([39m[38;5;252m)[39m[38;5;252m;[39m
[38;5;252m    [39m[38;5;252m}[39m
[38;5;252m}[39m

[38;5;246;03m// Final Classes[39;00m

[38;5;246;03m// Final Class declaration syntax[39;00m
[38;5;246;03m// <access-level> final <final-class-name> {[39;00m
[38;5;246;03m//     // Constants and variables[39;00m
[38;5;246;03m//     // Method declarations[39;00m
[38;5;246;03m// }[39;00m

[38;5;246;03m// Final classes are classes that cannot be inherited from and are therefore a[39;00m
[38;5;246;03m// final child. In a way, final classes are the opposite of abstract classes[39;00m
[38;5;246;03m// because abstract classes must be extended, but final classes cannot be[39;00m
[38;5;246;03m// extended.[39;00m
[38;5;70;01mpublic[39;00m[38;5;252m [39m[38;5;70;01mfinal[39;00m[38;5;252m [39m[38;5;70;01mclass[39;00m[38;5;252m [39m[38;5;68;04mSaberToothedCat[39;00m[38;5;252m [39m[38;5;70;01mextends[39;00m[38;5;252m [39m[38;5;252mAnimal[39m
[38;5;252m{[39m
[38;5;252m    [39m[38;5;246;03m// Note still have to override the abstract methods in the[39;00m
[38;5;252m    [39m[38;5;246;03m// abstract class.[39;00m
[38;5;252m    [39m[38;5;214m@Override[39m
[38;5;252m    [39m[38;5;70;01mpublic[39;00m[38;5;252m [39m[38;5;70;01mvoid[39;00m[38;5;252m [39m[38;5;68mmakeSound[39m[38;5;252m([39m[38;5;252m)[39m
[38;5;252m    [39m[38;5;252m{[39m
[38;5;252m        [39m[38;5;252mSystem[39m[38;5;252m.[39m[38;5;250mout[39m[38;5;252m.[39m[38;5;250mprintln[39m[38;5;252m([39m[38;5;214m"Roar"[39m[38;5;252m)[39m[38;5;252m;[39m
[38;5;252m    [39m[38;5;252m}[39m
[38;5;252m}[39m

[38;5;246;03m// Final Methods[39;00m
[38;5;70;01mpublic[39;00m[38;5;252m [39m[38;5;70;01mabstract[39;00m[38;5;252m [39m[38;5;70;01mclass[39;00m[38;5;252m [39m[38;5;68mMammal[39m[38;5;252m([39m[38;5;252m)[39m
[38;5;252m{[39m
[38;5;252m    [39m[38;5;246;03m// Final Method Syntax:[39;00m
[38;5;252m    [39m[38;5;246;03m// <access modifier> final <return type> <function name>(<args>)[39;00m

[38;5;252m    [39m[38;5;246;03m// Final methods, like, final classes cannot be overridden by a child[39;00m
[38;5;252m    [39m[38;5;246;03m// class, and are therefore the final implementation of the method.[39;00m
[38;5;252m    [39m[38;5;70;01mpublic[39;00m[38;5;252m [39m[38;5;70;01mfinal[39;00m[38;5;252m [39m[38;5;70;01mboolean[39;00m[38;5;252m [39m[38;5;68misWarmBlooded[39m[38;5;252m([39m[38;5;252m)[39m
[38;5;252m    [39m[38;5;252m{[39m
[38;5;252m        [39m[38;5;70;01mreturn[39;00m[38;5;252m [39m[38;5;70;01mtrue[39;00m[38;5;252m;[39m
[38;5;252m    [39m[38;5;252m}[39m
[38;5;252m}[39m

[38;5;246;03m// Enum Type[39;00m
[38;5;246;03m//[39;00m
[38;5;246;03m// An enum type is a special data type that enables for a variable to be a set[39;00m
[38;5;246;03m// of predefined constants. The variable must be equal to one of the values[39;00m
[38;5;246;03m// that have been predefined for it. Because they are constants, the names of[39;00m
[38;5;246;03m// an enum type's fields are in uppercase letters. In the Java programming[39;00m
[38;5;246;03m// language, you define an enum type by using the enum keyword. For example,[39;00m
[38;5;246;03m// you would specify a days-of-the-week enum type as:[39;00m
[38;5;70;01mpublic[39;00m[38;5;252m [39m[38;5;70;01menum[39;00m[38;5;252m [39m[38;5;252mDay[39m[38;5;252m [39m[38;5;252m{[39m
[38;5;252m    [39m[38;5;252mSUNDAY[39m[38;5;252m,[39m[38;5;252m [39m[38;5;252mMONDAY[39m[38;5;252m,[39m[38;5;252m [39m[38;5;252mTUESDAY[39m[38;5;252m,[39m[38;5;252m [39m[38;5;252mWEDNESDAY[39m[38;5;252m,[39m
[38;5;252m    [39m[38;5;252mTHURSDAY[39m[38;5;252m,[39m[38;5;252m [39m[38;5;252mFRIDAY[39m[38;5;252m,[39m[38;5;252m [39m[38;5;252mSATURDAY[39m
[38;5;252m}[39m

[38;5;246;03m// We can use our enum Day like that:[39;00m
[38;5;70;01mpublic[39;00m[38;5;252m [39m[38;5;70;01mclass[39;00m[38;5;252m [39m[38;5;68;04mEnumTest[39;00m[38;5;252m [39m[38;5;252m{[39m
[38;5;252m    [39m[38;5;246;03m// Variable Enum[39;00m
[38;5;252m    [39m[38;5;252mDay[39m[38;5;252m [39m[38;5;252mday[39m[38;5;252m;[39m

[38;5;252m    [39m[38;5;70;01mpublic[39;00m[38;5;252m [39m[38;5;68mEnumTest[39m[38;5;252m([39m[38;5;252mDay[39m[38;5;252m [39m[38;5;252mday[39m[38;5;252m)[39m[38;5;252m [39m[38;5;252m{[39m
[38;5;252m        [39m[38;5;70;01mthis[39;00m[38;5;252m.[39m[38;5;250mday[39m[38;5;252m [39m[38;5;252m=[39m[38;5;252m [39m[38;5;252mday[39m[38;5;252m;[39m
[38;5;252m    [39m[38;5;252m}[39m

[38;5;252m    [39m[38;5;70;01mpublic[39;00m[38;5;252m [39m[38;5;70;01mvoid[39;00m[38;5;252m [39m[38;5;68mtellItLikeItIs[39m[38;5;252m([39m[38;5;252m)[39m[38;5;252m [39m[38;5;252m{[39m
[38;5;252m        [39m[38;5;70;01mswitch[39;00m[38;5;252m [39m[38;5;252m([39m[38;5;252mday[39m[38;5;252m)[39m[38;5;252m [39m[38;5;252m{[39m
[38;5;252m            [39m[38;5;70;01mcase[39;00m[38;5;252m [39m[38;5;252mMONDAY[39m[38;5;252m:[39m
[38;5;252m                [39m[38;5;252mSystem[39m[38;5;252m.[39m[38;5;250mout[39m[38;5;252m.[39m[38;5;250mprintln[39m[38;5;252m([39m[38;5;214m"Mondays are bad."[39m[38;5;252m)[39m[38;5;252m;[39m
[38;5;252m                [39m[38;5;70;01mbreak[39;00m[38;5;252m;[39m
[38;5;252m            [39m[38;5;70;01mcase[39;00m[38;5;252m [39m[38;5;252mFRIDAY[39m[38;5;252m:[39m
[38;5;252m                [39m[38;5;252mSystem[39m[38;5;252m.[39m[38;5;250mout[39m[38;5;252m.[39m[38;5;250mprintln[39m[38;5;252m([39m[38;5;214m"Fridays are better."[39m[38;5;252m)[39m[38;5;252m;[39m
[38;5;252m                [39m[38;5;70;01mbreak[39;00m[38;5;252m;[39m
[38;5;252m            [39m[38;5;70;01mcase[39;00m[38;5;252m [39m[38;5;252mSATURDAY[39m[38;5;252m:[39m
[38;5;252m            [39m[38;5;70;01mcase[39;00m[38;5;252m [39m[38;5;252mSUNDAY[39m[38;5;252m:[39m
[38;5;252m                [39m[38;5;252mSystem[39m[38;5;252m.[39m[38;5;250mout[39m[38;5;252m.[39m[38;5;250mprintln[39m[38;5;252m([39m[38;5;214m"Weekends are best."[39m[38;5;252m)[39m[38;5;252m;[39m
[38;5;252m                [39m[38;5;70;01mbreak[39;00m[38;5;252m;[39m
[38;5;252m            [39m[38;5;70;01mdefault[39;00m[38;5;252m:[39m
[38;5;252m                [39m[38;5;252mSystem[39m[38;5;252m.[39m[38;5;250mout[39m[38;5;252m.[39m[38;5;250mprintln[39m[38;5;252m([39m[38;5;214m"Midweek days are so-so."[39m[38;5;252m)[39m[38;5;252m;[39m
[38;5;252m                [39m[38;5;70;01mbreak[39;00m[38;5;252m;[39m
[38;5;252m        [39m[38;5;252m}[39m
[38;5;252m    [39m[38;5;252m}[39m

[38;5;252m    [39m[38;5;70;01mpublic[39;00m[38;5;252m [39m[38;5;70;01mstatic[39;00m[38;5;252m [39m[38;5;70;01mvoid[39;00m[38;5;252m [39m[38;5;68mmain[39m[38;5;252m([39m[38;5;252mString[39m[38;5;252m[[39m[38;5;252m][39m[38;5;252m [39m[38;5;252margs[39m[38;5;252m)[39m[38;5;252m [39m[38;5;252m{[39m
[38;5;252m        [39m[38;5;252mEnumTest[39m[38;5;252m [39m[38;5;252mfirstDay[39m[38;5;252m [39m[38;5;252m=[39m[38;5;252m [39m[38;5;70;01mnew[39;00m[38;5;252m [39m[38;5;252mEnumTest[39m[38;5;252m([39m[38;5;252mDay[39m[38;5;252m.[39m[38;5;250mMONDAY[39m[38;5;252m)[39m[38;5;252m;[39m
[38;5;252m        [39m[38;5;252mfirstDay[39m[38;5;252m.[39m[38;5;250mtellItLikeItIs[39m[38;5;252m([39m[38;5;252m)[39m[38;5;252m;[39m[38;5;252m [39m[38;5;246;03m// => Mondays are bad.[39;00m
[38;5;252m        [39m[38;5;252mEnumTest[39m[38;5;252m [39m[38;5;252mthirdDay[39m[38;5;252m [39m[38;5;252m=[39m[38;5;252m [39m[38;5;70;01mnew[39;00m[38;5;252m [39m[38;5;252mEnumTest[39m[38;5;252m([39m[38;5;252mDay[39m[38;5;252m.[39m[38;5;250mWEDNESDAY[39m[38;5;252m)[39m[38;5;252m;[39m
[38;5;252m        [39m[38;5;252mthirdDay[39m[38;5;252m.[39m[38;5;250mtellItLikeItIs[39m[38;5;252m([39m[38;5;252m)[39m[38;5;252m;[39m[38;5;252m [39m[38;5;246;03m// => Midweek days are so-so.[39;00m
[38;5;252m    [39m[38;5;252m}[39m
[38;5;252m}[39m

[38;5;246;03m// Enum types are much more powerful than we show above.[39;00m
[38;5;246;03m// The enum body can include methods and other fields.[39;00m
[38;5;246;03m// You can see more at https://docs.oracle.com/javase/tutorial/java/javaOO/enum.html[39;00m

[38;5;246;03m// Getting Started with Lambda Expressions[39;00m
[38;5;246;03m//[39;00m
[38;5;246;03m// New to Java version 8 are lambda expressions. Lambdas are more commonly found[39;00m
[38;5;246;03m// in functional programming languages, which means they are methods which can[39;00m
[38;5;246;03m// be created without belonging to a class, passed around as if it were itself[39;00m
[38;5;246;03m// an object, and executed on demand.[39;00m
[38;5;246;03m//[39;00m
[38;5;246;03m// Final note, lambdas must implement a functional interface. A functional[39;00m
[38;5;246;03m// interface is one which has only a single abstract method declared. It can[39;00m
[38;5;246;03m// have any number of default methods. Lambda expressions can be used as an[39;00m
[38;5;246;03m// instance of that functional interface. Any interface meeting the requirements[39;00m
[38;5;246;03m// is treated as a functional interface. You can read more about interfaces[39;00m
[38;5;246;03m// above.[39;00m
[38;5;246;03m//[39;00m
[38;5;70;01mimport[39;00m[38;5;252m [39m[38;5;68;04mjava.util.Map[39;00m[38;5;252m;[39m
[38;5;70;01mimport[39;00m[38;5;252m [39m[38;5;68;04mjava.util.HashMap[39;00m[38;5;252m;[39m
[38;5;70;01mimport[39;00m[38;5;252m [39m[38;5;68;04mjava.util.function.*[39;00m[38;5;252m;[39m
[38;5;70;01mimport[39;00m[38;5;252m [39m[38;5;68;04mjava.security.SecureRandom[39;00m[38;5;252m;[39m

[38;5;70;01mpublic[39;00m[38;5;252m [39m[38;5;70;01mclass[39;00m[38;5;252m [39m[38;5;68;04mLambdas[39;00m[38;5;252m [39m[38;5;252m{[39m
[38;5;252m    [39m[38;5;70;01mpublic[39;00m[38;5;252m [39m[38;5;70;01mstatic[39;00m[38;5;252m [39m[38;5;70;01mvoid[39;00m[38;5;252m [39m[38;5;68mmain[39m[38;5;252m([39m[38;5;252mString[39m[38;5;252m[[39m[38;5;252m][39m[38;5;252m [39m[38;5;252margs[39m[38;5;252m)[39m[38;5;252m [39m[38;5;252m{[39m
[38;5;252m        [39m[38;5;246;03m// Lambda declaration syntax:[39;00m
[38;5;252m	[39m[38;5;246;03m// <zero or more parameters> -> <expression body or statement block>[39;00m

[38;5;252m        [39m[38;5;246;03m// We will use this hashmap in our examples below.[39;00m
[38;5;252m        [39m[38;5;252mMap[39m[38;5;252m<[39m[38;5;252mString[39m[38;5;252m,[39m[38;5;252m [39m[38;5;252mString[39m[38;5;252m>[39m[38;5;252m [39m[38;5;252mplanets[39m[38;5;252m [39m[38;5;252m=[39m[38;5;252m [39m[38;5;70;01mnew[39;00m[38;5;252m [39m[38;5;252mHashMap[39m[38;5;252m<[39m[38;5;252m>[39m[38;5;252m([39m[38;5;252m)[39m[38;5;252m;[39m
[38;5;252m            [39m[38;5;252mplanets[39m[38;5;252m.[39m[38;5;250mput[39m[38;5;252m([39m[38;5;214m"Mercury"[39m[38;5;252m,[39m[38;5;252m [39m[38;5;214m"87.969"[39m[38;5;252m)[39m[38;5;252m;[39m
[38;5;252m            [39m[38;5;252mplanets[39m[38;5;252m.[39m[38;5;250mput[39m[38;5;252m([39m[38;5;214m"Venus"[39m[38;5;252m,[39m[38;5;252m [39m[38;5;214m"224.7"[39m[38;5;252m)[39m[38;5;252m;[39m
[38;5;252m            [39m[38;5;252mplanets[39m[38;5;252m.[39m[38;5;250mput[39m[38;5;252m([39m[38;5;214m"Earth"[39m[38;5;252m,[39m[38;5;252m [39m[38;5;214m"365.2564"[39m[38;5;252m)[39m[38;5;252m;[39m
[38;5;252m            [39m[38;5;252mplanets[39m[38;5;252m.[39m[38;5;250mput[39m[38;5;252m([39m[38;5;214m"Mars"[39m[38;5;252m,[39m[38;5;252m [39m[38;5;214m"687"[39m[38;5;252m)[39m[38;5;252m;[39m
[38;5;252m            [39m[38;5;252mplanets[39m[38;5;252m.[39m[38;5;250mput[39m[38;5;252m([39m[38;5;214m"Jupiter"[39m[38;5;252m,[39m[38;5;252m [39m[38;5;214m"4,332.59"[39m[38;5;252m)[39m[38;5;252m;[39m
[38;5;252m            [39m[38;5;252mplanets[39m[38;5;252m.[39m[38;5;250mput[39m[38;5;252m([39m[38;5;214m"Saturn"[39m[38;5;252m,[39m[38;5;252m [39m[38;5;214m"10,759"[39m[38;5;252m)[39m[38;5;252m;[39m
[38;5;252m            [39m[38;5;252mplanets[39m[38;5;252m.[39m[38;5;250mput[39m[38;5;252m([39m[38;5;214m"Uranus"[39m[38;5;252m,[39m[38;5;252m [39m[38;5;214m"30,688.5"[39m[38;5;252m)[39m[38;5;252m;[39m
[38;5;252m            [39m[38;5;252mplanets[39m[38;5;252m.[39m[38;5;250mput[39m[38;5;252m([39m[38;5;214m"Neptune"[39m[38;5;252m,[39m[38;5;252m [39m[38;5;214m"60,182"[39m[38;5;252m)[39m[38;5;252m;[39m

[38;5;252m        [39m[38;5;246;03m// Lambda with zero parameters using the Supplier functional interface[39;00m
[38;5;252m        [39m[38;5;246;03m// from java.util.function.Supplier. The actual lambda expression is[39;00m
[38;5;252m        [39m[38;5;246;03m// what comes after numPlanets =.[39;00m
[38;5;252m        [39m[38;5;252mSupplier[39m[38;5;252m<[39m[38;5;252mString[39m[38;5;252m>[39m[38;5;252m [39m[38;5;252mnumPlanets[39m[38;5;252m [39m[38;5;252m=[39m[38;5;252m [39m[38;5;252m([39m[38;5;252m)[39m[38;5;252m [39m[38;5;252m-[39m[38;5;252m>[39m[38;5;252m [39m[38;5;252mInteger[39m[38;5;252m.[39m[38;5;250mtoString[39m[38;5;252m([39m[38;5;252mplanets[39m[38;5;252m.[39m[38;5;250msize[39m[38;5;252m([39m[38;5;252m)[39m[38;5;252m)[39m[38;5;252m;[39m
[38;5;252m        [39m[38;5;252mSystem[39m[38;5;252m.[39m[38;5;250mout[39m[38;5;252m.[39m[38;5;250mformat[39m[38;5;252m([39m[38;5;214m"Number of Planets: %s\n\n"[39m[38;5;252m,[39m[38;5;252m [39m[38;5;252mnumPlanets[39m[38;5;252m.[39m[38;5;250mget[39m[38;5;252m([39m[38;5;252m)[39m[38;5;252m)[39m[38;5;252m;[39m

[38;5;252m        [39m[38;5;246;03m// Lambda with one parameter and using the Consumer functional interface[39;00m
[38;5;252m        [39m[38;5;246;03m// from java.util.function.Consumer. This is because planets is a Map,[39;00m
[38;5;252m        [39m[38;5;246;03m// which implements both Collection and Iterable. The forEach used here,[39;00m
[38;5;252m        [39m[38;5;246;03m// found in Iterable, applies the lambda expression to each member of[39;00m
[38;5;252m        [39m[38;5;246;03m// the Collection. The default implementation of forEach behaves as if:[39;00m
[38;5;252m        [39m[38;5;246;03m/*[39;00m
[38;5;246;03m            for (T t : this)[39;00m
[38;5;246;03m                action.accept(t);[39;00m
[38;5;246;03m        */[39;00m

[38;5;252m        [39m[38;5;246;03m// The actual lambda expression is the parameter passed to forEach.[39;00m
[38;5;252m        [39m[38;5;252mplanets[39m[38;5;252m.[39m[38;5;250mkeySet[39m[38;5;252m([39m[38;5;252m)[39m[38;5;252m.[39m[38;5;250mforEach[39m[38;5;252m([39m[38;5;252m([39m[38;5;252mp[39m[38;5;252m)[39m[38;5;252m [39m[38;5;252m-[39m[38;5;252m>[39m[38;5;252m [39m[38;5;252mSystem[39m[38;5;252m.[39m[38;5;250mout[39m[38;5;252m.[39m[38;5;250mformat[39m[38;5;252m([39m[38;5;214m"%s\n"[39m[38;5;252m,[39m[38;5;252m [39m[38;5;252mp[39m[38;5;252m)[39m[38;5;252m)[39m[38;5;252m;[39m

[38;5;252m        [39m[38;5;246;03m// If you are only passing a single argument, then the above can also be[39;00m
[38;5;252m        [39m[38;5;246;03m// written as (note absent parentheses around p):[39;00m
[38;5;252m        [39m[38;5;252mplanets[39m[38;5;252m.[39m[38;5;250mkeySet[39m[38;5;252m([39m[38;5;252m)[39m[38;5;252m.[39m[38;5;250mforEach[39m[38;5;252m([39m[38;5;252mp[39m[38;5;252m [39m[38;5;252m-[39m[38;5;252m>[39m[38;5;252m [39m[38;5;252mSystem[39m[38;5;252m.[39m[38;5;250mout[39m[38;5;252m.[39m[38;5;250mformat[39m[38;5;252m([39m[38;5;214m"%s\n"[39m[38;5;252m,[39m[38;5;252m [39m[38;5;252mp[39m[38;5;252m)[39m[38;5;252m)[39m[38;5;252m;[39m

[38;5;252m        [39m[38;5;246;03m// Tracing the above, we see that planets is a HashMap, keySet() returns[39;00m
[38;5;252m        [39m[38;5;246;03m// a Set of its keys, forEach applies each element as the lambda[39;00m
[38;5;252m        [39m[38;5;246;03m// expression of: (parameter p) -> System.out.format("%s\n", p). Each[39;00m
[38;5;252m        [39m[38;5;246;03m// time, the element is said to be "consumed" and the statement(s)[39;00m
[38;5;252m        [39m[38;5;246;03m// referred to in the lambda body is applied. Remember the lambda body[39;00m
[38;5;252m        [39m[38;5;246;03m// is what comes after the ->.[39;00m

[38;5;252m        [39m[38;5;246;03m// The above without use of lambdas would look more traditionally like:[39;00m
[38;5;252m        [39m[38;5;70;01mfor[39;00m[38;5;252m [39m[38;5;252m([39m[38;5;252mString[39m[38;5;252m [39m[38;5;252mplanet[39m[38;5;252m [39m[38;5;252m:[39m[38;5;252m [39m[38;5;252mplanets[39m[38;5;252m.[39m[38;5;250mkeySet[39m[38;5;252m([39m[38;5;252m)[39m[38;5;252m)[39m[38;5;252m [39m[38;5;252m{[39m
[38;5;252m            [39m[38;5;252mSystem[39m[38;5;252m.[39m[38;5;250mout[39m[38;5;252m.[39m[38;5;250mformat[39m[38;5;252m([39m[38;5;214m"%s\n"[39m[38;5;252m,[39m[38;5;252m [39m[38;5;252mplanet[39m[38;5;252m)[39m[38;5;252m;[39m
[38;5;252m        [39m[38;5;252m}[39m

[38;5;252m        [39m[38;5;246;03m// This example differs from the above in that a different forEach[39;00m
[38;5;252m        [39m[38;5;246;03m// implementation is used: the forEach found in the HashMap class[39;00m
[38;5;252m        [39m[38;5;246;03m// implementing the Map interface. This forEach accepts a BiConsumer,[39;00m
[38;5;252m        [39m[38;5;246;03m// which generically speaking is a fancy way of saying it handles[39;00m
[38;5;252m        [39m[38;5;246;03m// the Set of each Key -> Value pairs. This default implementation[39;00m
[38;5;252m        [39m[38;5;246;03m// behaves as if:[39;00m
[38;5;252m        [39m[38;5;246;03m/*[39;00m
[38;5;246;03m            for (Map.Entry<K, V> entry : map.entrySet())[39;00m
[38;5;246;03m                action.accept(entry.getKey(), entry.getValue());[39;00m
[38;5;246;03m        */[39;00m

[38;5;252m        [39m[38;5;246;03m// The actual lambda expression is the parameter passed to forEach.[39;00m
[38;5;252m        [39m[38;5;252mString[39m[38;5;252m [39m[38;5;252morbits[39m[38;5;252m [39m[38;5;252m=[39m[38;5;252m [39m[38;5;214m"%s orbits the Sun in %s Earth days.\n"[39m[38;5;252m;[39m
[38;5;252m        [39m[38;5;252mplanets[39m[38;5;252m.[39m[38;5;250mforEach[39m[38;5;252m([39m[38;5;252m([39m[38;5;252mK[39m[38;5;252m,[39m[38;5;252m [39m[38;5;252mV[39m[38;5;252m)[39m[38;5;252m [39m[38;5;252m-[39m[38;5;252m>[39m[38;5;252m [39m[38;5;252mSystem[39m[38;5;252m.[39m[38;5;250mout[39m[38;5;252m.[39m[38;5;250mformat[39m[38;5;252m([39m[38;5;252morbits[39m[38;5;252m,[39m[38;5;252m [39m[38;5;252mK[39m[38;5;252m,[39m[38;5;252m [39m[38;5;252mV[39m[38;5;252m)[39m[38;5;252m)[39m[38;5;252m;[39m

[38;5;252m        [39m[38;5;246;03m// The above without use of lambdas would look more traditionally like:[39;00m
[38;5;252m        [39m[38;5;70;01mfor[39;00m[38;5;252m [39m[38;5;252m([39m[38;5;252mString[39m[38;5;252m [39m[38;5;252mplanet[39m[38;5;252m [39m[38;5;252m:[39m[38;5;252m [39m[38;5;252mplanets[39m[38;5;252m.[39m[38;5;250mkeySet[39m[38;5;252m([39m[38;5;252m)[39m[38;5;252m)[39m[38;5;252m [39m[38;5;252m{[39m
[38;5;252m            [39m[38;5;252mSystem[39m[38;5;252m.[39m[38;5;250mout[39m[38;5;252m.[39m[38;5;250mformat[39m[38;5;252m([39m[38;5;252morbits[39m[38;5;252m,[39m[38;5;252m [39m[38;5;252mplanet[39m[38;5;252m,[39m[38;5;252m [39m[38;5;252mplanets[39m[38;5;252m.[39m[38;5;250mget[39m[38;5;252m([39m[38;5;252mplanet[39m[38;5;252m)[39m[38;5;252m)[39m[38;5;252m;[39m
[38;5;252m        [39m[38;5;252m}[39m

[38;5;252m        [39m[38;5;246;03m// Or, if following more closely the specification provided by the[39;00m
[38;5;252m        [39m[38;5;246;03m// default implementation:[39;00m
[38;5;252m        [39m[38;5;70;01mfor[39;00m[38;5;252m [39m[38;5;252m([39m[38;5;252mMap[39m[38;5;252m.[39m[38;5;250mEntry[39m[38;5;252m<[39m[38;5;252mString[39m[38;5;252m,[39m[38;5;252m [39m[38;5;252mString[39m[38;5;252m>[39m[38;5;252m [39m[38;5;252mplanet[39m[38;5;252m [39m[38;5;252m:[39m[38;5;252m [39m[38;5;252mplanets[39m[38;5;252m.[39m[38;5;250mentrySet[39m[38;5;252m([39m[38;5;252m)[39m[38;5;252m)[39m[38;5;252m [39m[38;5;252m{[39m
[38;5;252m            [39m[38;5;252mSystem[39m[38;5;252m.[39m[38;5;250mout[39m[38;5;252m.[39m[38;5;250mformat[39m[38;5;252m([39m[38;5;252morbits[39m[38;5;252m,[39m[38;5;252m [39m[38;5;252mplanet[39m[38;5;252m.[39m[38;5;250mgetKey[39m[38;5;252m([39m[38;5;252m)[39m[38;5;252m,[39m[38;5;252m [39m[38;5;252mplanet[39m[38;5;252m.[39m[38;5;250mgetValue[39m[38;5;252m([39m[38;5;252m)[39m[38;5;252m)[39m[38;5;252m;[39m
[38;5;252m        [39m[38;5;252m}[39m

[38;5;252m        [39m[38;5;246;03m// These examples cover only the very basic use of lambdas. It might not[39;00m
[38;5;252m        [39m[38;5;246;03m// seem like much or even very useful, but remember that a lambda can be[39;00m
[38;5;252m        [39m[38;5;246;03m// created as an object that can later be passed as parameters to other[39;00m
[38;5;252m        [39m[38;5;246;03m// methods.[39;00m
[38;5;252m    [39m[38;5;252m}[39m
[38;5;252m}[39m
